<!-- This is the resource manager                                           -->
<PAGE
  PAGE_ID="ProviderManagement_facilityMgrHome"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.TitleText"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="WorkAllocation"
    NAME="DISPLAY"
    OPERATION="listTaskAndActivitiesForCurrentUser"
  />
  <SERVER_INTERFACE
    CLASS="Provider"
    NAME="DISPLAYPROVIDER"
    OPERATION="listProvidersForFacilityManager"
  />
  <CLUSTER
    NUM_COLS="2"
    SHOW_LABELS="false"
    STYLE="outer-cluster-borderless"
  >
    <LIST TITLE="ActionControl.Label.Providers">
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAYPROVIDER"
            PROPERTY="name"
          />
        </CONNECT>
        <LINK PAGE_ID="ProviderManagement_providerHome">
          <CONNECT>
            <SOURCE
              NAME="DISPLAYPROVIDER"
              PROPERTY="concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="concernRoleID"
            />
          </CONNECT>
          <!-- BEGIN, CR00246089, PS -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAYPROVIDER"
              PROPERTY="concernRoleID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="providerConcernRoleID"
            />
          </CONNECT>
          <!-- END, CR00246089 -->
        </LINK>
      </FIELD>
    </LIST>
    <LIST TITLE="ActionControl.Label.MyTasks">
      <FIELD
        LABEL="Field.Title.TaskID"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="taskID"
          />
        </CONNECT>
        <LINK PAGE_ID="TaskManagement_taskHome">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="taskID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="taskID"
            />
          </CONNECT>
        </LINK>
      </FIELD>
      <FIELD
        LABEL="Field.Title.Subject"
        WIDTH="60"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="reservedTasks$dtlsList$dtls$subject"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Title.Deadline"
        WIDTH="20"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="dueDateTime"
          />
        </CONNECT>
      </FIELD>
    </LIST>
    <CLUSTER
      NUM_COLS="1"
      SHOW_LABELS="false"
      TITLE="ActionControl.Label.Shortcuts"
    >
      <CONTAINER>
        <ACTION_CONTROL LABEL="ActionControl.Label.ChangePassword">
          <LINK PAGE_ID="Organization_modifyUserPassword"/>
        </ACTION_CONTROL>
      </CONTAINER>
    </CLUSTER>
    <LIST TITLE="ActionControl.Label.Calendar">
      <FIELD
        LABEL="Field.Title.StartDate"
        WIDTH="30"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="startDateTime"
          />
        </CONNECT>
      </FIELD>
      <FIELD
        LABEL="Field.Title.Subject"
        WIDTH="70"
      >
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="intervalDetailsList$subject"
          />
        </CONNECT>
        <LINK PAGE_ID="Activity_resolveApplicationHome">
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="activityID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="ID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="activityTypeCode"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="TYPE"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="recurringInd"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="RE"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="activityLevel"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="LV"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="attendeeInd"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="AT"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="acceptanceProvisionalInd"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="AC"
            />
          </CONNECT>
        </LINK>
      </FIELD>
    </LIST>
  </CLUSTER>
</PAGE>
