<!-- This page allows the user to modify an existing reservation -->
<!-- BEGIN, CR00260666, ASN -->
<?curam-deprecated Since Curam 6.0 SP1,as this page is redundant and not in use anymore.
  Edit reservation page is merged to display and update the information as a part of single page. 
  Information present in this page is included in ProviderManagement_modifyReservationPeriodAndServiceFromList1 page.
  Hence, this page is deprecated. See release note: CR00260666 ?>
<!-- END, CR00260666 -->
<PAGE
  PAGE_ID="ProviderManagement_modifyPlaceForReservationFromList"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <!-- BEGIN, CR00236219, PS -->
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>
  <!-- END, CR00236219 -->


  <SERVER_INTERFACE
    CLASS="MaintainReservation"
    NAME="ACTION"
    OPERATION="updateReservation"
    PHASE="ACTION"
  />
  <SERVER_INTERFACE
    CLASS="MaintainReservation"
    NAME="DISPLAY"
    OPERATION="viewReservation"
    PHASE="DISPLAY"
  />


  <PAGE_PARAMETER NAME="reservationID"/>
  <PAGE_PARAMETER NAME="from"/>
  <PAGE_PARAMETER NAME="to"/>
  <PAGE_PARAMETER NAME="versionNo"/>
  <PAGE_PARAMETER NAME="providerOfferingID"/>
  <PAGE_PARAMETER NAME="concernRoleID"/>
  <INCLUDE FILE_NAME="ProviderManagement_modifyPlaceForReservationView.vim"/>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    />


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>


  </ACTION_SET>
</PAGE>
