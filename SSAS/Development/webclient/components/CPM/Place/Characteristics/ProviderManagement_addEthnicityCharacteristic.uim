<!--To add ethnicity characteristic                                         -->
<PAGE
  PAGE_ID="ProviderManagement_addEthnicityCharacteristic"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Home"
      />
    </CONNECT>
  </PAGE_TITLE>
  <SERVER_INTERFACE
    CLASS="Compartment"
    NAME="DISPLAY"
    OPERATION="getEthnicityListForUpdate"
    PHASE="DISPLAY"
  />
  <SERVER_INTERFACE
    CLASS="Compartment"
    NAME="ACTION"
    OPERATION="addEthnicityCharacteristics"
    PHASE="ACTION"
  />
  <PAGE_PARAMETER NAME="compartmentCharacteristicID"/>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="compartmentCharacteristicID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$characteristicID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="compartmentCharacteristicID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="ethnicityDetails$placementCharacteristic$characteristicID"
    />
  </CONNECT>
  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save1"
      TYPE="SUBMIT"
    />
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Cancel"
      TYPE="ACTION"
    />
  </ACTION_SET>


  <!-- BEGIN, CR00248428, GP -->
  <CLUSTER SHOW_LABELS="false">
    <FIELD CONTROL="TRANSFER_LIST">
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="result$ethnicityDetailsList$ethnicityCode"
          NAME="DISPLAY"
          PROPERTY="result$ethnicityDetailsList$ethnicityDescription"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="ethnicityDetails$ethnicityIDTabList"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!-- END, CR00248428 -->
</PAGE>
