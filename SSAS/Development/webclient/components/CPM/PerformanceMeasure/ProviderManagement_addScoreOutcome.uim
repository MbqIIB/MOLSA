<!-- This page allows the user to add a score outcome.                      -->
<PAGE
  PAGE_ID="ProviderManagement_addScoreOutcome"
  WINDOW_OPTIONS="width=500"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ProviderManagement_addScoreOutcome"/>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <SERVER_INTERFACE
    CLASS="MaintainSOPerformanceMeasure"
    NAME="ACTION"
    OPERATION="addScoreOutcome"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="performanceMeasureID"/>
  <SERVER_INTERFACE
    CLASS="MaintainPerformanceMeasure"
    NAME="DISPLAY1"
    OPERATION="viewPerformanceMeasure"
    PHASE="DISPLAY"
  />


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="performanceMeasureID"
    />
    <TARGET
      NAME="DISPLAY1"
      PROPERTY="key$performanceMeasureID"
    />
  </CONNECT>


  <PAGE_PARAMETER NAME="soPerformanceMeasureLinkID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="soPerformanceMeasureLinkID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="soPerformanceMeasureLinkID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="53"
    NUM_COLS="2"
  >
    <FIELD LABEL="Field.Label.MinimumScore">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="minimumScore"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Outcome">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="outcome"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.MaximumScore">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="maximumScore"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


</PAGE>
