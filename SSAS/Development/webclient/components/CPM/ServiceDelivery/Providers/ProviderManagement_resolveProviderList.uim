<PAGE
  PAGE_ID="ProviderManagement_resolveProviderList"
  WINDOW_OPTIONS="width=400"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>
  <JSP_SCRIPTLET>
        
  
  curam.omega3.request.RequestHandler 
           rh = curam.omega3.request.RequestHandlerFactory.getRequestHandler(request);
     
  String context = request.getContextPath() + "/";
      context += curam.omega3.user.UserPreferencesFactory.getUserPreferences(pageContext.getSession()).getLocale() + "/";
  
  String serviceDeliveryID = request.getParameter("serviceDeliveryID");
  
  if (serviceDeliveryID == null) {
   core.ScriptletMissingParamException e = new
            core.ScriptletMissingParamException(-20002, "serviceDeliveryID");
   throw e;
  }
    
  String url = "";
  
  // BEGIN, CR00376993, PS
  url = context + "ProviderManagement_listProvidersPage.do?" + "serviceDeliveryID=" + curam.omega3.request.RequestUtils.escapeURL(serviceDeliveryID)
  + "&amp;addressID=0&amp;providerSpecialtiesTabbedString= " + "&amp;proximityDistance= "+ "&amp;providerName= ";
  // END, CR00376993
  
  url += "&amp;" + rh.getSystemParameters();
  
  response.sendRedirect(response.encodeRedirectURL(url));      
       
       
  
    </JSP_SCRIPTLET>
</PAGE>
