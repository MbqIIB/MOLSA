      <IS_TRUE
        NAME="DISPLAY"
        PROPERTY="concerningModifiableInd"
      />
    </CONDITION>
    <!--BEGIN CR00119203, PP-->
    <FIELD
      LABEL="Field.Label.Concerning"
      WIDTH="100"
      WIDTH_UNITS="PERCENT"
    >
      <!--END, CR00237116-->
      <!--END CR00119203, PP-->
      <CONNECT>
        <INITIAL
          HIDDEN_PROPERTY="concernRoleID"
          NAME="DISPLAY"
          PROPERTY="concernRoleName"
        />
      </CONNECT>
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="concerningID"
        />
      </CONNECT>
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="concerningID"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER NUM_COLS="2">
    <CONDITION>
      <IS_FALSE
        NAME="DISPLAY"
        PROPERTY="concerningModifiableInd"
      />
    </CONDITION>
    <FIELD LABEL="Field.Label.Concerning">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="concerningName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
  <!--END CR00109143-->


