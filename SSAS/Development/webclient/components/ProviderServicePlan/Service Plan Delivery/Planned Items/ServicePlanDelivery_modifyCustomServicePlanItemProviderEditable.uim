    </FIELD>
    <FIELD LABEL="Field.Label.GuidanceURL">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$plannedItemDetails$plannedItemDetails$plannedItemDtls$guidanceURL"
