<!-- This workflow is used to approve or deny a roster line item.           -->
<workflow-process category="PC21" createdBy="admin" creationDate="20110502T181650" id="28524" language-version="1.0" process-version="2" released="true">
  <name>ROSTERLINEITEMAPPROVALWORKFLOW</name>
  <process-display-name>
    <localized-text>
      <locale language="ar">مسار العمل لاعتماد بند اللائحة</locale>
      <locale language="en">Roster Line Item Approval Workflow</locale>
    </localized-text>
  </process-display-name>
  <description>
    <localized-text>
      <locale language="ar">عملية مسار العمل لاعتماد بند اللائحة لجهة التقديم.</locale>
      <locale language="en">Provider roster line item approval workflow process.</locale>
    </localized-text>
  </description>
  <enactment-mappings>
    <mapping>
      <source-attribute name="providerRosterLineItemID" struct-name="curam.cpm.workflowprocesses.attendancetracking.struct.RosterLineItemApprovalDescriptor" />
      <target-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
    </mapping>
  </enactment-mappings>
  <wdos>
    <wdo initialize-attributes="false" is-list-wdo="false">
      <wdo-name>PRLIApprovalDescriptor</wdo-name>
      <display-name>
        <localized-text>
          <locale language="ar">PRLI Exception Processing Descriptor</locale>
          <locale language="en">PRLI Exception Processing Descriptor</locale>
        </localized-text>
      </display-name>
      <attributes>
        <attribute>
          <attribute-name>providerRosterLineItemID</attribute-name>
          <display-name>
            <localized-text>
              <locale language="ar">كود بند اللائحة لجهة التقديم</locale>
              <locale language="en">Provider Roster Line Item ID</locale>
            </localized-text>
          </display-name>
          <type>INT64</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>referenceNumber</attribute-name>
          <display-name>
            <localized-text>
              <locale language="ar">رقم المرجع لبند اللائحة لجهة التقديم</locale>
              <locale language="en">Reference Number of the Provider Roster Line Item</locale>
            </localized-text>
          </display-name>
          <type>STRING</type>
          <required-at-enactment>false</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
      </attributes>
    </wdo>
  </wdos>
  <activities>
    <start-process-activity id="1">
      <name>
        <localized-text>
          <locale language="ar">بدء نشاط العملية</locale>
          <locale language="en">Start Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="ar">بدء نشاط العملية</locale>
          <locale language="en">The start process activity</locale>
        </localized-text>
      </description>
      <split type="none">
        <transition-id idref="1" />
      </split>
    </start-process-activity>
    <end-process-activity id="2">
      <name>
        <localized-text>
          <locale language="ar">نشاط انهاء العملية</locale>
          <locale language="en">End Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="ar">نشاط انهاء العملية</locale>
          <locale language="en">The end process activity</locale>
        </localized-text>
      </description>
      <join type="none" />
    </end-process-activity>
    <automatic-activity category="AC1" id="4">
      <name>
        <localized-text>
          <locale language="ar">احضار رقم المرجع لبند اللائحة لجهة التقديم</locale>
          <locale language="en">Get Reference Number for Provider Roster Line Item</locale>
        </localized-text>
      </name>
      <join type="none" />
      <split type="none">
        <transition-id idref="3" />
      </split>
      <bpo-mapping interface-name="curam.cpm.workflowprocesses.attendancetracking.intf.RosterWorkflowAllocation" method-name="readReferenceNumberForPRLI">
        <formal-parameters>
          <formal-parameter index="0">
            <struct type="curam.cpm.sl.entity.struct.ProviderRosterLineItemKey">
              <field name="providerRosterLineItemID">
                <base-type type="INT64">
                  <wdo-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
                </base-type>
              </field>
            </struct>
          </formal-parameter>
        </formal-parameters>
        <return>
          <struct>
            <field name="referenceNumber">
              <base-type>
                <wdo-attribute name="referenceNumber" wdo-name="PRLIApprovalDescriptor" />
              </base-type>
            </field>
          </struct>
        </return>
      </bpo-mapping>
    </automatic-activity>
    <manual-activity category="AC1" id="3">
      <name>
        <localized-text>
          <locale language="ar">حدد الاختيار لاعتماد أو رفض بند اللائحة لجهة التقديم</locale>
          <locale language="en">Choose to approve or deny Provider Roster Line Item</locale>
        </localized-text>
      </name>
      <notification delivery-mechanism="DM1">
        <subject>
          <message>
            <message-text>
              <localized-text>
                <locale language="ar">بند اللائحة لجهة التقديم المحال للاعتماد</locale>
                <locale language="en">Provider Roster Line Item submitted for approval</locale>
              </localized-text>
            </message-text>
          </message>
        </subject>
        <body>
          <message>
            <message-text>
              <localized-text>
                <locale language="ar">تم احالة بند اللائحة لجهة التقديم برقم المرجع، %1 للاعتماد.</locale>
                <locale language="en">The Provider Roster Line Item with reference number %1 has been submitted for approval.</locale>
              </localized-text>
            </message-text>
            <message-parameters>
              <wdo-attribute name="referenceNumber" wdo-name="PRLIApprovalDescriptor" />
            </message-parameters>
          </message>
        </body>
        <allocation-strategy identifier="curam.cpm.workflowprocesses.intf.WorkflowAllocationFunction.prliSubmissionTaskAllocationStrategy" type="function">
          <function-mappings>
            <formal-parameters>
              <formal-parameter index="0">
                <struct type="curam.cpm.sl.entity.struct.PRLIRefKey">
                  <field name="rliID">
                    <base-type type="STRING">
                      <wdo-attribute name="referenceNumber" wdo-name="PRLIApprovalDescriptor" />
                    </base-type>
                  </field>
                </struct>
              </formal-parameter>
            </formal-parameters>
          </function-mappings>
        </allocation-strategy>
      </notification>
      <join type="none" />
      <split type="none">
        <transition-id idref="2" />
      </split>
      <task>
        <message>
          <message-text>
            <localized-text>
              <locale language="ar">تم احالة بند اللائحة لجهة التقديم برقم المرجع، %1s، للاعتماد.</locale>
              <locale language="en">The provider roster line item with reference number, %1s, has been submitted for approval.</locale>
            </localized-text>
          </message-text>
          <message-parameters>
            <wdo-attribute name="referenceNumber" wdo-name="PRLIApprovalDescriptor" />
          </message-parameters>
        </message>
        <actions>
          <action open-modal="false" page-id="ProviderManagement_approveordenyPRLI" principal-action="true">
            <message>
              <message-text>
                <localized-text>
                  <locale language="ar">اعتماد بند اللائحة لجهة التقديم: للاستمرار لتوضيح أنه يجب اعتماد أو رفض بند اللائحة لجهة التقديم</locale>
                  <locale language="en">Provider Roster Line Item Approval: Proceed to indicate that the provider roster line item should is approved or denied</locale>
                </localized-text>
              </message-text>
            </message>
            <link-parameter name="providerRosterLineItemID">
              <wdo-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
            </link-parameter>
          </action>
          <action open-modal="false" page-id="ProviderManagement_viewRosterLineItemFrmTask" principal-action="false">
            <message>
              <message-text>
                <localized-text>
                  <locale language="ar">الصفحة الرئيسية لبند اللائحة لجهة التقديم</locale>
                  <locale language="en">Provider Roster Line Item Home</locale>
                </localized-text>
              </message-text>
            </message>
            <link-parameter name="providerRosterLineItemID">
              <wdo-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
            </link-parameter>
          </action>
        </actions>
        <allow-deadline-override>false</allow-deadline-override>
        <allow-task-forwarding>false</allow-task-forwarding>
      </task>
      <allocation-strategy identifier="curam.cpm.workflowprocesses.intf.WorkflowAllocationFunction.prliSubmissionTaskAllocationStrategy" type="function">
        <function-mappings>
          <formal-parameters>
            <formal-parameter index="0">
              <struct type="curam.cpm.sl.entity.struct.PRLIRefKey">
                <field name="rliID">
                  <base-type type="STRING">
                    <wdo-attribute name="referenceNumber" wdo-name="PRLIApprovalDescriptor" />
                  </base-type>
                </field>
              </struct>
            </formal-parameter>
          </formal-parameters>
        </function-mappings>
      </allocation-strategy>
      <event-wait wait-on-all-events="false">
        <events>
          <event event-class="ROSTER" event-type="PRLI_APPROVED" identifier="3">
            <event-match-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
          </event>
          <event event-class="ROSTER" event-type="PRLI_DENIED" identifier="4">
            <event-match-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
          </event>
        </events>
      </event-wait>
    </manual-activity>
  </activities>
  <transitions>
    <transition from-activity-idref="1" id="1" to-activity-idref="4" />
    <transition from-activity-idref="3" id="2" to-activity-idref="2" />
    <transition from-activity-idref="4" id="3" to-activity-idref="3" />
  </transitions>
</workflow-process>

