<!--BEGIN, CR00235681, PM-->
<workflow-process xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" category="PC21" id="12" language-version="1.0" process-version="1" released="true" xsi:noNamespaceSchemaLocation="../../../CuramSDEJ_ZIP/lib/Workflow.xsd">
  <!--  END, CR00235681-->
  <name>ROSTERLINEITEMAPPROVAL</name>
  <process-display-name>
    <localized-text>
      <locale language="ar">اعتماد بند اللائحة</locale>
      <locale language="en">Roster Line Item Approval</locale>
    </localized-text>
  </process-display-name>
  <!-- BEGIN, CR00248687, NS -->
  <description>
    <localized-text>
      <locale language="ar">تقوم هذه العملية بتكوين مهمة لصف العمل عندما يتطلب بند اللائحة الاعتماد اليدوي والوصول الى الحالة 'تعليق الاعتماد'. أي مستخدم قام بالاشتراك في صف العمل هذا يمكنه مشاهدة وحجز المهمة.</locale>
      <locale language="en">This process creates a task for a work queue when a roster line item requires manual approval and has reached a 'Pending Approval' status. Any user who has subscribed to this work queue can view and reserve the task.</locale>
    </localized-text>
  </description>
  <!-- END, CR00248687 -->
  <enactment-mappings>
    <mapping>
      <source-attribute name="providerRosterLineItemID" struct-name="curam.cpm.workflowprocesses.attendancetracking.struct.RosterLineItemApprovalDescriptor" />
      <target-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
    </mapping>
    <mapping>
      <source-attribute name="referenceNumber" struct-name="curam.cpm.workflowprocesses.attendancetracking.struct.RosterLineItemApprovalDescriptor" />
      <target-attribute name="referenceNumber" wdo-name="PRLIApprovalDescriptor" />
    </mapping>
  </enactment-mappings>
  <wdos>
    <wdo initialize-attributes="false" is-list-wdo="false">
      <wdo-name>PRLIApprovalDescriptor</wdo-name>
      <display-name>
        <localized-text>
          <locale language="ar">PRLI Exception Processing Descriptor</locale>
          <locale language="en">PRLI Exception Processing Descriptor</locale>
        </localized-text>
      </display-name>
      <attributes>
        <attribute>
          <attribute-name>providerRosterLineItemID</attribute-name>
          <display-name>
            <localized-text>
              <locale language="ar">كود بند اللائحة لجهة التقديم</locale>
              <locale language="en">Provider Roster Line Item ID</locale>
            </localized-text>
          </display-name>
          <type>INT64</type>
          <required-at-enactment>true</required-at-enactment>
        </attribute>
        <attribute>
          <attribute-name>referenceNumber</attribute-name>
          <display-name>
            <localized-text>
              <locale language="ar">رقم المرجع لبند اللائحة لجهة التقديم</locale>
              <locale language="en">Reference Number of the Provider Roster Line Item</locale>
            </localized-text>
          </display-name>
          <type>STRING</type>
          <required-at-enactment>true</required-at-enactment>
        </attribute>
      </attributes>
    </wdo>
  </wdos>
  <activities>
    <start-process-activity id="1">
      <name>
        <localized-text>
          <locale language="ar">بدء نشاط العملية</locale>
          <locale language="en">Start Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="ar">بدء نشاط العملية</locale>
          <locale language="en">The start process activity</locale>
        </localized-text>
      </description>
      <split type="none">
        <transition-id idref="1" />
      </split>
    </start-process-activity>
    <end-process-activity id="2">
      <name>
        <localized-text>
          <locale language="ar">نشاط انهاء العملية</locale>
          <locale language="en">End Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="ar">نشاط انهاء العملية</locale>
          <locale language="en">The end process activity</locale>
        </localized-text>
      </description>
      <join type="none" />
    </end-process-activity>
    <manual-activity category="AC1" id="3">
      <name>
        <localized-text>
          <locale language="ar">حدد الاختيار لاعتماد أو رفض بند اللائحة لجهة التقديم</locale>
          <locale language="en">Choose to approve or deny Provider Roster Line Item</locale>
        </localized-text>
      </name>
      <join type="none" />
      <split type="none">
        <transition-id idref="2" />
      </split>
      <task>
        <message>
          <message-text>
            <localized-text>
              <locale language="ar">تم احالة بند اللائحة لجهة التقديم برقم المرجع، %1s، للاعتماد.</locale>
              <locale language="en">The provider roster line item with reference
                number, %1s, has been submitted for approval.</locale>
            </localized-text>
          </message-text>
          <message-parameters>
            <wdo-attribute name="referenceNumber" wdo-name="PRLIApprovalDescriptor" />
          </message-parameters>
        </message>
        <actions>
          <action page-id="ProviderManagement_approveordenyPRLI" principal-action="true">
            <message>
              <message-text>
                <localized-text>
                  <locale language="ar">اعتماد بند اللائحة لجهة التقديم: للاستمرار لتوضيح أنه يجب اعتماد أو رفض بند اللائحة لجهة التقديم</locale>
                  <locale language="en">Provider Roster Line Item Approval: Proceed to indicate that the provider roster line item should is approved or denied</locale>
                </localized-text>
              </message-text>
            </message>
            <link-parameter name="providerRosterLineItemID">
              <wdo-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
            </link-parameter>
          </action>
          <action page-id="ProviderManagement_viewRosterLineItemFrmTask" principal-action="false">
            <message>
              <message-text>
                <localized-text>
                  <locale language="ar">الصفحة الرئيسية لبند اللائحة لجهة التقديم</locale>
                  <locale language="en">Provider Roster Line Item Home</locale>
                </localized-text>
              </message-text>
            </message>
            <link-parameter name="providerRosterLineItemID">
              <wdo-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
            </link-parameter>
          </action>
        </actions>
      </task>
      <!--BEGIN, CR00138066, RPB -->
      <allocation-strategy identifier="Roster Line Item Approval Work Queue" type="target" />
      <!--END, CR00138066 -->
      <event-wait wait-on-all-events="false">
        <events>
          <event event-class="ROSTER" event-type="PRLI_APPROVED" identifier="3">
            <event-match-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
          </event>
          <event event-class="ROSTER" event-type="PRLI_DENIED" identifier="4">
            <event-match-attribute name="providerRosterLineItemID" wdo-name="PRLIApprovalDescriptor" />
          </event>
        </events>
      </event-wait>
    </manual-activity>
  </activities>
  <transitions>
    <transition from-activity-idref="1" id="1" to-activity-idref="3" />
    <transition from-activity-idref="3" id="2" to-activity-idref="2" />
  </transitions>
</workflow-process>

