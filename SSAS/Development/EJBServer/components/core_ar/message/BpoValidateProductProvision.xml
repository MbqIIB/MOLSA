<messages package="curam.message">
  <message name="ERR_PRODPROVISION_XFV_DATE_RANGE">
    <locale language="ar">يجب أن يكون تاريخ بدء وتاريخ انتهاء اعداد المنتج في مدى تاريخ البدء وتاريخ الانتهاء للمنتج.</locale>
  </message>
  <message name="ERR_PRODPROVISION_FV_CANCELLED_PRODUCT">
    <locale language="ar">لا يمكن تكوين اعداد المنتج لأن اعداد المنتج لا يمكن أن يكون مرافق لمنتج تم الغائه.</locale>
  </message>
  <message name="ERR_PRODPROVISION_FV_CREATION_DATE_EMPTY">
    <locale language="ar">يجب ادخال تاريخ التكوين.</locale>
  </message>
  <message name="ERR_PRODPROVISION_FV_FREQUENCY_EMPTY">
    <locale language="ar">يجب ادخال تكرار المدفوعات.</locale>
  </message>
  <message name="ERR_PRODPROVISION_FV_CURRENCY_EMPTY">
    <locale language="ar">يجب ادخال نوع العملة.</locale>
  </message>
  <message name="ERR_PRODPROVISION_FV_PAY_METHOD_EMPTY">
    <locale language="ar">يجب ادخال طريقة السداد.</locale>
  </message>
