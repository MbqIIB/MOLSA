<messages package="curam.clientdiary.message">
  <message name="INFO_BLANK_STRING">
    <!--  START NON-TRANSLATABLE  -->
    <locale language="ar" />
    <!--  END NON-TRANSLATABLE  -->
  </message>
  <message name="INFO_DATE_STRING">
    <!--  START NON-TRANSLATABLE  -->
    <locale language="ar">%1d</locale>
    <!--  END NON-TRANSLATABLE  -->
  </message>
  <message name="INFO_DATE_TIME_STRING">
    <!--  START NON-TRANSLATABLE  -->
    <locale language="ar">%1z</locale>
    <!--  END NON-TRANSLATABLE  -->
  </message>
  <!-- Used to display the duration of an appointment. E.g. 1 hour(s) and 30 minute(s) -->
  <message name="INFO_DURATION_STRING_HOURS_MINUTES">
    <locale language="ar">%1n ساعة(ساعات) %2n دقيقة(دقائق)</locale>
  </message>
  <!-- Used to display the duration of an appointment where a frequency is also given. E.g. Daily for 1 hour(s) and 30 minute(s) -->
  <message name="INFO_FREQUENCY_DURATION_STRING">
    <locale language="ar">لمدة %1n ساعة(ساعات) %2n دقيقة(دقائق)</locale>
  </message>
  <!-- Used to display the recurrence of an appointment where a frequency is also given. E.g. Daily for 2 occurrances from 1/1/2001 -->
  <message name="INFO_FREQUENCY_RECURRENCE_NUM_OCCURRENCES_START_DATE_STRING">
    <locale language="ar">لعدد %1n من التكرارات من %2d</locale>
  </message>
  <!-- Used to display the recurrence of an appointment where a frequency is also given. E.g. Daily from 1/1/2001 to 2/2/2001 -->
  <message name="INFO_FREQUENCY_RECURRENCE_DATE_RANGE_STRING">
    <locale language="ar">من %1d الى %2d</locale>
  </message>
</messages>

