<!-- This workflow is used to generate a service invoice exception task.    -->
<workflow-process
  category="PC21"
  createdBy="admin"
  creationDate="20090821T174507"
  id="28607"
  language-version="1.0"
  process-version="1"
  released="true"
>
  <name>SUPERVISORREQUESTDECISIONWORKFLOW</name>


  <process-display-name>
    <localized-text>
      <locale language="en">Supervisor Request Decision Workflow</locale>
    </localized-text>
  </process-display-name>


  <!-- BEGIN, CR00306765, AKr -->
  <description>
    <localized-text>
      <locale language="en">Notifies a user that an external user has submitted a request to create a new account.</locale>
    </localized-text>
  </description>
  <!-- END, CR00306765 -->
  <enactment-mappings>
    <mapping>
      <source-attribute
        name="requestID"
        struct-name="curam.cpm.eua.facade.struct.ExternalRequestSubmittedDetails"
      />
      <target-attribute
        name="requestID"
        wdo-name="ExternalTaskCreatedDetails"
      />
    </mapping>
  </enactment-mappings>
  <wdos>
    <wdo
      initialize-attributes="false"
      is-list-wdo="false"
    >
      <wdo-name>ExternalTaskCreatedDetails</wdo-name>
      <display-name>
        <localized-text>
          <locale language="en">ExternalTaskCreatedDetails</locale>
        </localized-text>
      </display-name>
      <attributes>
        <attribute>
          <attribute-name>requestID</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Request ID</locale>
            </localized-text>
          </display-name>
          <type>INT64</type>
          <required-at-enactment>true</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>requestCategory</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Request Category</locale>
            </localized-text>
          </display-name>
          <type>STRING</type>
          <required-at-enactment>false</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>concernRoleID</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">Concern Role ID</locale>
            </localized-text>
          </display-name>
          <type>INT64</type>
          <required-at-enactment>false</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
        <attribute>
          <attribute-name>requestType</attribute-name>
          <display-name>
            <localized-text>
              <locale language="en">requestType</locale>
            </localized-text>
          </display-name>
          <type>STRING</type>
          <required-at-enactment>false</required-at-enactment>
          <process-output>false</process-output>
        </attribute>
      </attributes>
    </wdo>
  </wdos>
  <activities>
    <start-process-activity id="1">
      <name>
        <localized-text>
          <locale language="en">Start Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="en">The start process activity</locale>
        </localized-text>
      </description>
      <split type="none">
        <transition-id idref="1"/>
      </split>
    </start-process-activity>
    <end-process-activity id="2">
      <name>
        <localized-text>
          <locale language="en">End Process Activity</locale>
        </localized-text>
      </name>
      <description>
        <localized-text>
          <locale language="en">The end process activity</locale>
        </localized-text>
      </description>
      <join type="none"/>
    </end-process-activity>
    <automatic-activity
      category="AC1"
      id="4"
    >
      <name>
        <localized-text>
          <locale language="en">Get Request Category And Type Details</locale>
        </localized-text>
      </name>
      <join type="none"/>
      <split type="none">
        <transition-id idref="3"/>
      </split>
      <bpo-mapping
        interface-name="curam.cpm.workflowprocesses.intf.ExternalUserWorkflowAllocation"
        method-name="readRequestCategoryAndTypeDetailsForExternalRequest"
      >
        <formal-parameters>
          <formal-parameter index="0">
            <struct type="curam.cpm.eua.facade.struct.RequestKey">
              <field name="requestKey">
                <struct type="curam.cpm.eua.entity.struct.RequestKey">
                  <field name="requestID">
                    <base-type type="INT64">
                      <wdo-attribute
                        name="requestID"
                        wdo-name="ExternalTaskCreatedDetails"
                      />
                    </base-type>
                  </field>
                </struct>
              </field>
            </struct>
          </formal-parameter>
        </formal-parameters>
        <return>
          <struct>
            <field name="requestCategory">
              <base-type>
                <wdo-attribute
                  name="requestCategory"
                  wdo-name="ExternalTaskCreatedDetails"
                />
              </base-type>
            </field>
            <field name="requestType">
              <base-type>
                <wdo-attribute
                  name="requestType"
                  wdo-name="ExternalTaskCreatedDetails"
                />
              </base-type>
            </field>
            <field name="concernRoleID">
              <base-type>
                <wdo-attribute
                  name="concernRoleID"
                  wdo-name="ExternalTaskCreatedDetails"
                />
              </base-type>
            </field>
          </struct>
        </return>
      </bpo-mapping>
    </automatic-activity>
    <manual-activity
      category="AC1"
      id="3"
    >
      <name>
        <localized-text>
          <locale language="en">Assigned Owner</locale>
        </localized-text>
      </name>
      <join type="none"/>
      <split type="none">
        <transition-id idref="2"/>
      </split>
      <task>
        <message>
          <message-text>
            <localized-text>
              <locale language="en">%1c has been submitted and needs to be processed</locale>
            </localized-text>
          </message-text>
          <message-parameters>
            <wdo-attribute
              name="requestCategory"
              wdo-name="ExternalTaskCreatedDetails"
            />
          </message-parameters>
        </message>
        <actions>
          <action
            page-id="ProviderManagement_resolveViewExtAccRequest"
            principal-action="true"
          >
            <message>
              <message-text>
                <localized-text>
                  <locale language="en">Request Home</locale>
                </localized-text>
              </message-text>
            </message>
            <link-parameter name="requestID">
              <wdo-attribute
                name="requestID"
                wdo-name="ExternalTaskCreatedDetails"
              />
            </link-parameter>
            <link-parameter name="requestCategory">
              <wdo-attribute
                name="requestCategory"
                wdo-name="ExternalTaskCreatedDetails"
              />
            </link-parameter>
          </action>
          <action
            page-id="ProviderManagement_listServiceInvoicesForProvider"
            principal-action="false"
          >
            <message>
              <message-text>
                <localized-text>
                  <locale language="en">Service Invoice Home</locale>
                </localized-text>
              </message-text>
            </message>
            <link-parameter name="concernRoleID">
              <wdo-attribute
                name="concernRoleID"
                wdo-name="ExternalTaskCreatedDetails"
              />
            </link-parameter>
          </action>
        </actions>
        <task-priority>
          <priority>TP2</priority>
        </task-priority>
        <allow-deadline-override>false</allow-deadline-override>
        <allow-task-forwarding>true</allow-task-forwarding>
      </task>
      <allocation-strategy
        identifier="External Request Work Queue"
        type="target"
      />
      <event-wait wait-on-all-events="false">
        <events>
          <event
            event-class="REQUESTDECISION"
            event-type="REQUESTACCEPTED"
            identifier="1"
          >
            <event-match-attribute
              name="requestID"
              wdo-name="ExternalTaskCreatedDetails"
            />
          </event>
          <event
            event-class="REQUESTDECISION"
            event-type="REQUESTREJECTED"
            identifier="2"
          >
            <event-match-attribute
              name="requestID"
              wdo-name="ExternalTaskCreatedDetails"
            />
          </event>
        </events>
      </event-wait>
    </manual-activity>
  </activities>
  <transitions>
    <transition
      from-activity-idref="1"
      id="1"
      to-activity-idref="4"
    />
    <transition
      from-activity-idref="3"
      id="2"
      to-activity-idref="2"
    />
    <transition
      from-activity-idref="4"
      id="3"
      to-activity-idref="3"
    />
  </transitions>
</workflow-process>
