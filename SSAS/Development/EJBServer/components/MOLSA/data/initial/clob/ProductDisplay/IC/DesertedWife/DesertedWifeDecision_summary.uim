<?xml version="1.0" encoding="ISO-8859-1"?>
<PAGE PAGE_ID="DesertedWifeDecision_summary">
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="Page.Title"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="CREOLEProgramRecommendation"
    NAME="DISPLAY"
    OPERATION="viewDisplayRulesCategoryXML"
  />


  <PAGE_PARAMETER NAME="creoleProgramRecommendationID"/>
  <PAGE_PARAMETER NAME="simulatedDeterminationID"/>
  <PAGE_PARAMETER NAME="date"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="creoleProgramRecommendationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="creoleProgramRecommendationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="simulatedDeterminationID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="simulatedDeterminationID"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="date"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="date"
    />
  </CONNECT>
  <CONNECT>
    <SOURCE
      NAME="TEXT"
      PROPERTY="categoryRef"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="categoryRef"
    />
  </CONNECT>


  <CLUSTER NUM_COLS="2">
    <CLUSTER>
      <CLUSTER TITLE="Cluster.Title.AssistanceUnit">
        <CONDITION>
          <IS_TRUE
            EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/doesHouseholdHaveEligibleMembersTimeline"
            NAME="DISPLAY"
            PROPERTY="result$decisionDetailsXML"
          />
        </CONDITION>
        <LIST>
          <FIELD LABEL="Field.Label.HouseholdMember">
            <CONNECT>
              <SOURCE
                EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/eligibilityUnitMemberDetails/Item[]"
                NAME="DISPLAY"
                PROPERTY="result$decisionDetailsXML"
              />
            </CONNECT>
          </FIELD>
          <FIELD LABEL="Field.Label.Reason">
            <CONNECT>
              <SOURCE
                EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/eligibilityUnitMembersReason/Item[]"
                NAME="DISPLAY"
                PROPERTY="result$decisionDetailsXML"
              />
            </CONNECT>
          </FIELD>
        </LIST>
      </CLUSTER>
    </CLUSTER>
    
	<!--excluded members-->
	
  <CLUSTER TITLE="Cluster.Title.ExcludedHouseholdMember">
  <CONDITION>
    <IS_TRUE
      EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/doesHouseholdHaveExcludedMembersTimeline"
      NAME="DISPLAY"
      PROPERTY="result$decisionDetailsXML"
    />
  </CONDITION>
  <LIST >
    <FIELD LABEL="Field.Label.ExcludedHouseholdMember">
      <CONNECT>
        <SOURCE
          EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/excludedUnitMemberDetails/Item[]"
          NAME="DISPLAY"
          PROPERTY="result$decisionDetailsXML"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Reason">
      <CONNECT>
        <SOURCE
          EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/excludedUnitMembersReason/Item[]"
          NAME="DISPLAY"
          PROPERTY="result$decisionDetailsXML"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</CLUSTER>

	
	
	<!--eligible-->
	<CLUSTER TITLE="Cluster.Title.Eligibility">
      <CLUSTER STYLE="cgiss-display-removeMargin">
        <FIELD LABEL="Field.Label.NonFinancial">
          <CONNECT>
            <SOURCE
              EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/hasHouseholdPassedNonFinancialMessage"
              NAME="DISPLAY"
              PROPERTY="result$decisionDetailsXML"
            />
          </CONNECT>
          <LINK PAGE_ID="SADecision_nonFinancial">
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="creoleProgramRecommendationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="creoleProgramRecommendationID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="simulatedDeterminationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="simulatedDeterminationID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="date"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="date"
              />
            </CONNECT>
          </LINK>
        </FIELD>
        <FIELD LABEL="Field.Label.Income">
          <CONNECT>
            <SOURCE
              EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/hasHouseholdPassedIncomeMessage"
              NAME="DISPLAY"
              PROPERTY="result$decisionDetailsXML"
            />
          </CONNECT>
          <LINK PAGE_ID="SADecision_income">
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="creoleProgramRecommendationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="creoleProgramRecommendationID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="simulatedDeterminationID"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="simulatedDeterminationID"
              />
            </CONNECT>
            <CONNECT>
              <SOURCE
                NAME="PAGE"
                PROPERTY="date"
              />
              <TARGET
                NAME="PAGE"
                PROPERTY="date"
              />
            </CONNECT>
          </LINK>
        </FIELD>
      </CLUSTER>
    </CLUSTER>
	
	<!--Ineligible members-->

    <CLUSTER TITLE="Cluster.Title.InEligibleHouseholdMember">
  <CONDITION>
    <IS_TRUE
      EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/doesHouseholdHaveInEligibleMembersTimeline"
      NAME="DISPLAY"
      PROPERTY="result$decisionDetailsXML"
    />
  </CONDITION>
  <LIST >
    <FIELD LABEL="Field.Label.InEligibleHouseholdMember">
      <CONNECT>
        <SOURCE
          EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/inEligibilityUnitMemberDetails/Item[]"
          NAME="DISPLAY"
          PROPERTY="result$decisionDetailsXML"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Reason">
      <CONNECT>
        <SOURCE
          EXTENDED_PATH="/DecisionDetails/DesertedWifeSummaryCategory/inEligibilityUnitMembersReason/Item[]"
          NAME="DISPLAY"
          PROPERTY="result$decisionDetailsXML"
        />
      </CONNECT>
    </FIELD>
  </LIST>
</CLUSTER>

    
  </CLUSTER>


</PAGE>
