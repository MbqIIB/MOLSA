<RuleSet name="AnonymousParentsRuleSet">
    <Class extends="DynamicEvidence" extendsRuleSet="DynamicEvidenceRuleSet" name="AnonymousParents">
        <Attribute name="anonymousParents">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Boolean"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="participant">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Number"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="related_participant">
            <Annotations/>
            <type>
                <ruleclass name="CaseParticipantRole" ruleset="CaseEntitiesRuleSet"/>
            </type>
            <derivation>
                <reference attribute="caseParticipantRoleRecord">
                    <create ruleclass="CaseParticipantRoleCalculator"
                        ruleset="DynamicEvidenceRuleSet">
                        <reference attribute="participant"/>
                    </create>
                </reference>
            </derivation>
        </Attribute>
    </Class>
</RuleSet>
