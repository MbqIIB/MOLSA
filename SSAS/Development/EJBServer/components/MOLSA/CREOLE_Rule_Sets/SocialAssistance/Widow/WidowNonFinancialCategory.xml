<?xml version="1.0" encoding="UTF-8"?>
<Class xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:noNamespaceSchemaLocation="../../../../../../../../../../../Curam/catalog/RuleSet.xsd"
 extends="SANonFinancialCategory"
 extendsRuleSet="SocialAssistanceDisplayRuleSet" name="WidowNonFinancialCategory">

  <Attribute name="productCase">
    <type>
      <ruleclass name="WidowProductCase" ruleset="WidowEligibilityAndEntitlementRuleSet"/>
    </type>
    <derivation>
      <create
        ruleclass="WidowProductCase"
        ruleset="WidowEligibilityAndEntitlementRuleSet"
        >
        <specify attribute="productDeliveryCase">
          <reference attribute="productDeliveryCase"/>
        </specify>
      </create>
    </derivation>
  </Attribute>
  
  <Attribute name="eligibilityUnitMembersTimeline">
    <Annotations>
      <Display/>
    </Annotations>
    <type>
      <javaclass name="curam.creole.value.Timeline">
        <javaclass name="List">
          <ruleclass
            name="CaseParticipantRole"
            ruleset="CaseEntitiesRuleSet"
          />
        </javaclass>
      </javaclass>
    </type>
    <derivation>
      <timelineoperation>
        <dynamiclist>
          <list>
            <intervalvalue>
              <reference attribute="eligibleMembersTimeline">
                <reference attribute="eligibilityUnit">
                  <reference attribute="productCase"/>
                </reference>
              </reference>
            </intervalvalue>
          </list>
          <listitemexpression>
            <reference attribute="caseParticipantRoleRecord">
              <current/>
            </reference>
          </listitemexpression>
        </dynamiclist>
      </timelineoperation>
    </derivation>
  </Attribute>

