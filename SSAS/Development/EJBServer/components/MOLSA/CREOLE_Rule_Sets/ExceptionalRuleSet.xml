<RuleSet name="ExceptionalRuleSet">
    <Class extends="DynamicEvidence" extendsRuleSet="DynamicEvidenceRuleSet" name="Exceptional">
        <Attribute name="amount">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Number"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="suspendedDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="caseType">
            <Annotations/>
            <type>
                <codetableentry table="MOLSAExceptionEvideceType"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="startDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="isSuspended">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Boolean"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="deductionStartDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="deductionAmount">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Number"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="endDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="programType">
            <Annotations/>
            <type>
                <codetableentry table="MOLSAExcpPgmType"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="deductionEndDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="participant">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Number"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="related_participant">
            <Annotations/>
            <type>
                <ruleclass name="CaseParticipantRole" ruleset="CaseEntitiesRuleSet"/>
            </type>
            <derivation>
                <reference attribute="caseParticipantRoleRecord">
                    <create ruleclass="CaseParticipantRoleCalculator"
                        ruleset="DynamicEvidenceRuleSet">
                        <reference attribute="participant"/>
                    </create>
                </reference>
            </derivation>
        </Attribute>
    </Class>
</RuleSet>
