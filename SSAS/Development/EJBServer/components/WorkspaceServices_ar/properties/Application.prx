  <property name="curam.workspaceservices.disable.dyn.ev.val.inital.int.app.processing" dynamic="yes" constant="ENV_WORKSPACESERVICES_DISABLE_DYN_EV_VALIDATION_INITIAL_INT_APP_PROCESSING">
