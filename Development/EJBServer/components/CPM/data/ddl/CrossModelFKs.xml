<root>
  <foreignkeys>
    <key>
      <association
        constraintname="FKCRLERulSetLnkCrlRulSet"
        othertablename="CreoleRuleSet"
        tablename="CREOLERuleSetLink"
      >
        <foreignkeypair
          localfield="creoleRuleSetID"
          remotefield="creoleRuleSetID"
        />
      </association>


      <association
        constraintname="FKSpltyTypIcnAppResource"
        othertablename="AppResource"
        tablename="SpecialtyTypeIcon"
      >
        <foreignkeypair
          localfield="resourceID"
          remotefield="resourceID"
        />
      </association>
      <association
        constraintname="FKRosterProviderOffering"
        othertablename="ProviderOffering"
        tablename="Roster"
      >
        <foreignkeypair
          localfield="serviceID"
          remotefield="providerOfferingID"
        />
      </association>


      <association
        constraintname="FKSOSecRightsSecIdntifier"
        othertablename="SecurityIdentifier"
        tablename="SOSecurityRights"
      >
        <foreignkeypair
          localfield="sidName"
          remotefield="sidName"
        />
      </association>


    </key>
  </foreignkeys>
</root>
