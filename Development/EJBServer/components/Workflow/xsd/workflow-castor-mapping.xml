<mapping>
  <description>Mapping of XML file to Workflow Processes in Castor</description>
  <class name="curam.util.internal.workflow.metadata.impl.ProcessDefinition">
    <map-to xml="workflow-process"/>
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="version">
      <bind-xml name="process-version" node="attribute"/>
    </field>
    <field name="languageVersion">
      <bind-xml name="language-version" node="attribute"/>
    </field>
    <field name="released">
      <bind-xml name="released" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="createdBy">
      <bind-xml name="createdBy" node="attribute"/>
    </field>
    <field name="creationDate" set-method="setCreationDateTimeAsString" get-method="getCreationDateTimeAsString">
      <bind-xml name="creationDate" node="attribute"/>
    </field>
    <field name="name">
      <bind-xml name="name" node="element"/>
    </field>
    <field name="displayName">
      <bind-xml name="process-display-name" node="element"/>
    </field>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="documentation">
      <bind-xml name="documentation" node="element"/>
    </field>
    <field name="webService">
      <bind-xml name="web-service" node="element"/>
    </field>
    <field name="failureAllocationStrategy">
      <bind-xml name="allocation-strategy" node="element" location="failure-allocation-strategy"/>
    </field>   
    <field name="enactmentMappings" collection="arraylist" set-method="setEnactmentMappings" get-method="getEnactmentMappings" type="curam.util.internal.workflow.metadata.impl.Mapping">
      <bind-xml name="mapping" node="element" location="enactment-mappings"/>
    </field>
    <field name="wdos" collection="arraylist" set-method="setWDOs" get-method="getWDOs" type="curam.util.internal.workflow.metadata.impl.WDODefinition">
      <bind-xml name="wdo" node="element" location="wdos"/>
    </field>
    <!-- ACTIVITY TYPES -->
    <field name="startProcessActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.StartProcessActivity">
      <bind-xml name="start-process-activity" node="element" location="activities"/>
    </field>
    <field name="endProcessActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.EndProcessActivity">
      <bind-xml name="end-process-activity" node="element" location="activities"/>
    </field>
    <field name="automaticActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.AutomaticActivity">
      <bind-xml name="automatic-activity" node="element" location="activities"/>
    </field>
    <field name="manualActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.ManualActivity">
      <bind-xml name="manual-activity" node="element" location="activities"/>
    </field>
    <field name="decisionActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.DecisionActivity">
      <bind-xml name="decision-activity" node="element" location="activities"/>
    </field>
    <field name="subflowActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.SubflowActivity">
      <bind-xml name="subflow-activity" node="element" location="activities"/>
    </field>
    <field name="routeActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.RouteActivity">
      <bind-xml name="route-activity" node="element" location="activities"/>
    </field>
    <field name="loopBeginActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.LoopBeginActivity">
      <bind-xml name="loop-begin-activity" node="element" location="activities"/>
    </field>
    <field name="loopEndActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.LoopEndActivity">
      <bind-xml name="loop-end-activity" node="element" location="activities"/>
    </field>
    <field name="eventWaitActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.EventWaitActivity">
      <bind-xml name="event-wait-activity" node="element" location="activities"/>
    </field>
    <field name="parallelActivities" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.ParallelActivity">
      <bind-xml name="parallel-activity" node="element" location="activities"/>
    </field>
    <!-- END ACTIVITY TYPES -->
    <field name="transitions" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.Transition">
      <bind-xml name="transition" node="element" location="transitions"/>
    </field>
  </class>
  <!-- AUTOMATIC ACTIVITY -->
  <class name="curam.util.internal.workflow.metadata.impl.AutomaticActivity">
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="name"/>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="notification">
      <bind-xml name="notification" node="element"/>
    </field>
    <field name="joinType">
      <bind-xml name="join" node="element"/>
    </field>
    <field name="splitType">
      <bind-xml name="split" node="element"/>
    </field>
    <field name="mapping">
      <bind-xml name="bpo-mapping" node="element"/>
    </field>
  </class>
  <!-- END AUTOMATIC ACTIVITY -->
  <!-- MANUAL ACTIVITY -->
  <class name="curam.util.internal.workflow.metadata.impl.ManualActivity">
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="name"/>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="notification">
      <bind-xml name="notification" node="element"/>
    </field>
    <field name="joinType">
      <bind-xml name="join" node="element"/>
    </field>
    <field name="splitType">
      <bind-xml name="split" node="element"/>
    </field>
    <field name="activityTask">
      <bind-xml name="task"/>
    </field>
    <field name="allocationStrategyData">
      <bind-xml name="allocation-strategy"/>
    </field>
    <field name="eventWaitData">
      <bind-xml name="event-wait"/>
    </field>
    <field name="bizObjectAssociations" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.BizObjectAssociation">
      <bind-xml name="biz-object-association" node="element" location="biz-object-associations"/>
    </field>
  </class>
  <!-- END MANUAL ACTIVITY -->
  <!-- DECISION ACTIVITY -->
  <class name="curam.util.internal.workflow.metadata.impl.DecisionActivity">
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="name"/>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="notification">
      <bind-xml name="notification" node="element"/>
    </field>
    <field name="joinType">
      <bind-xml name="join" node="element"/>
    </field>
    <field name="splitType">
      <bind-xml name="split" node="element"/>
    </field>
    <field name="allocationStrategyData">
      <bind-xml name="allocation-strategy"/>
    </field>
    <field name="message"/>
    <field name="decisionAction">
      <bind-xml name="decision-action"/>
    </field>
    <field name="secondaryActions" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.SecondaryAction">
      <bind-xml name="secondary-action" node="element" location="secondary-actions"/>
    </field>
    <field name="question">
      <bind-xml name="question"/>
    </field>
    <field name="deadline">
      <bind-xml name="deadline" node="element"/>
    </field>
    <field name="reminders" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.Reminder">
      <bind-xml name="reminder" node="element" location="reminders"/>
    </field>
  </class>
  <!-- END DECISION ACTIVITY -->
  <!-- DECISION ACTION -->
  <class name="curam.util.internal.workflow.metadata.impl.DecisionAction">
    <map-to xml="decision-action"/>
    <field name="message"/>
  </class>
  <!-- END DECISION ACTION -->
  <!-- SECONDARY ACTION -->
  <class name="curam.util.internal.workflow.metadata.impl.SecondaryAction">
    <field name="pageId">
      <bind-xml name="page-id" node="attribute"/>
    </field>
    <field name="openModalInd">
      <bind-xml name="open-modal" node="attribute"/>
    </field>    
    <field name="message"/>
    <field name="linkParameters" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.LinkParameter">
      <bind-xml name="link-parameter" node="element"/>
    </field>
    <field name="multipleOccurringAction">
      <bind-xml name="multiple-occurring-action" node="element"/>
    </field>
  </class>
  <!-- END SECONDARY ACTION -->  
  <!-- QUESTION -->
  <class name="curam.util.internal.workflow.metadata.impl.Question">
    <map-to xml="question"/>
    <field name="message"/>
    <field name="freeTextAnswer">
      <bind-xml name="free-text" node="element"/>
    </field>
    <field name="multipleChoiceAnswer">
      <bind-xml name="answers" node="element"/>
    </field>
  </class>
  <!-- END QUESTION -->
  <!-- FREE TEXT ANSWER -->
  <class name="curam.util.internal.workflow.metadata.impl.FreeTextAnswer">
    <map-to xml="free-text"/>
    <field name="type">
      <bind-xml name="type" node="attribute"/>
    </field>
    <field name="wdoAttribute" set-method="setWDOAttribute" get-method="getWDOAttribute">
      <bind-xml name="wdo-attribute" node="element"/>
    </field>
  </class>
  <!-- END FREE TEXT ANSWER -->
  <!-- MULTIPLE CHOICE ANSWER -->
  <class name="curam.util.internal.workflow.metadata.impl.MultipleChoiceAnswer">
    <map-to xml="answers"/>
    <field name="multipleSelection">
      <bind-xml name="multiple-selection" node="attribute"/>
    </field>
    <field name="answers" collection="arraylist" set-method="setAnswers" get-method="getAnswers" type="curam.util.internal.workflow.metadata.impl.Answer">
      <bind-xml name="answer" node="element"/>
    </field>
  </class>
  <!-- END MULTIPLE CHOICE ANSWER -->
  <!-- CHOICE OUTPUT MAPPING -->
  <class name="curam.util.internal.workflow.metadata.impl.ChoiceOutputMapping">
    <map-to xml="choice-output-mapping"/>
    <field name="wdoAttribute" set-method="setWDOAttribute" get-method="getWDOAttribute">
      <bind-xml name="wdo-attribute" node="element"/>
    </field>
    <field name="selectedValue">
      <bind-xml name="selected-value" node="element"/>
    </field>
    <field name="notSelectedValue">
      <bind-xml name="not-selected-value" node="element"/>
    </field>
  </class>
  <!-- END CHOICE OUTPUT MAPPING -->
  <!-- ANSWER -->
  <class name="curam.util.internal.workflow.metadata.impl.Answer">
    <map-to xml="answer"/>
    <field name="answerText"/>
    <field name="name">
      <bind-xml name="name" node="attribute"/>
    </field>
    <field name="choiceOutputMapping"/>
  </class>
  <!-- END ANSWER -->
  <!-- ALLOCATION STRATEGY -->
  <class name="curam.util.internal.workflow.metadata.impl.AllocationStrategy">
    <map-to xml="allocation-strategy"/>
    <field name="type" set-method="setType" get-method="getAllocationTypeAsString">
      <bind-xml name="type" node="attribute"/>
    </field>
    <field name="castorType" set-method="setType" get-method="getNullType"/>
    <field name="identifier">
      <bind-xml name="identifier" node="attribute"/>
    </field>
    <field name="rulesetMappings" collection="arraylist"
           type="curam.util.internal.workflow.metadata.impl.RDOMapping">
      <bind-xml name="rdo-mapping" node="element"
                location="ruleset-mappings"/>
    </field>
    <field name="cerSetMappings" set-method="setCERSetMappings" get-method="getCERSetMappings">
      <bind-xml name="cer-set-mappings" node="element"/>
    </field>    
    <field name="functionMappings">
      <bind-xml name="function-mappings" node="element"/>
    </field>
  </class>
  <!-- END ALLOCATION STRATEGY -->
  <!-- FUNCTION MAPPINGS -->
  <class name="curam.util.internal.workflow.metadata.impl.FunctionMappings">
    <field name="formalParameters" collection="arraylist"
           type="curam.util.internal.workflow.metadata.impl.FormalParameter">
      <bind-xml name="formal-parameter" node="element"
                location="formal-parameters"/>
    </field>
  </class>
  <!-- END FUNCTION MAPPINGS -->
  <!-- CER SET MAPPINGS -->
  <class name="curam.util.internal.workflow.metadata.impl.CERSetMappings">
    <field name="primaryClass">
      <bind-xml name="primary-class" node="attribute"/>
    </field>  
    <field name="cerClassMappings" collection="arraylist"
      type="curam.util.internal.workflow.metadata.impl.CERClassMapping"
      set-method="setCERClassMappings" get-method="getCERClassMappings">
      <bind-xml name="cer-class-mapping" node="element"/>
    </field>
  </class>
  <!-- END CER SET MAPPINGS -->  
  <!-- END RULESET MAPPINGS -->
  <!-- RDO MAPPING -->
  <class name="curam.util.internal.workflow.metadata.impl.RDOMapping">
    <field name="sourceAttribute">
      <bind-xml name="source-attribute" node="element"/>
    </field>
    <field name="rdoTargetAttribute">
      <bind-xml name="target-attribute" node="element"/>
    </field>
  </class>
  <!-- END RDO MAPPING -->
  <!-- CER CLASS MAPPING -->
  <class name="curam.util.internal.workflow.metadata.impl.CERClassMapping">
    <field name="sourceAttribute">
      <bind-xml name="source-attribute" node="element"/>
    </field>
    <field name="cerClassTargetAttribute" 
      set-method="setCERClassTargetAttribute" get-method="getCERClassTargetAttribute">
      <bind-xml name="target-attribute" node="element"/>
    </field>
  </class>
  <!-- END CER CLASS MAPPING -->  
  <!-- EVENT WAIT -->
  <class name="curam.util.internal.workflow.metadata.impl.EventWaitData">
    <map-to xml="event-wait"/>
    <field name="waitOnAllEvents">
      <bind-xml name="wait-on-all-events" node="attribute"/>
    </field>    
    <field name="events" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.Event">
      <bind-xml name="event" node="element" location="events"/>
    </field>
    <field name="eventOutputMappings">
      <bind-xml name="event-output-mappings" node="element"/>
    </field>
    <field name="eventWaitDeadlineData">
      <bind-xml name="deadline" node="element"/>
    </field>
    <field name="reminders" collection="arraylist" type="curam.util.internal.workflow.metadata.impl.Reminder">
       <bind-xml name="reminder" node="element" location="reminders"/>
    </field>
  </class>
  <!-- END EVENT WAIT -->
  <!-- EVENT -->
  <class name="curam.util.internal.workflow.metadata.impl.Event">
    <map-to xml="event"/>
    <field name="identifier">
      <bind-xml name="identifier" node="attribute"/>
    </field>
    <field name="eventClass">
      <bind-xml name="event-class" node="attribute"/>
    </field>
    <field name="eventType">
      <bind-xml name="event-type" node="attribute"/>
    </field>
    <field name="eventData">
      <bind-xml name="event-match-attribute" node="element"/>
    </field>
    <field name="multipleOccurringEvent">
      <bind-xml name="multiple-occurring-event" node="element"/>
    </field>    
  </class>
  <!-- END EVENT -->
  <!-- EVENT OUTPUT MAPPINGS -->
  <class name="curam.util.internal.workflow.metadata.impl.EventOutputMappings">
    <map-to xml="event-output-mappings"/>
    <field name="eventType">
      <bind-xml name="event-type" node="element"/>
    </field>
    <field name="eventOutputData">
      <bind-xml name="output-data" node="element"/>
    </field>
    <field name="eventRaisedBy">
      <bind-xml name="raised-by" node="element"/>
    </field>
    <field name="eventTimeRaised">
      <bind-xml name="time-raised" node="element"/>
    </field>
  </class>
  <!-- END EVENT OUTPUT MAPPINGS -->
   <!-- REMINDER -->
    <class name="curam.util.internal.workflow.metadata.impl.Reminder">
      <map-to xml="reminder"/>
      <field name="identifier">
        <bind-xml name="id" node="attribute"/>
      </field>
      <field name="deliveryOffset">
        <bind-xml name="delivery-offset" node="attribute"/>
      </field>
      <field name="deliveryTime">
         <bind-xml name="delivery-time" node="element"/>
      </field>
      <field name="notification">
        <bind-xml name="notification" node="element"/>
      </field>
    </class>
  <!-- REMINDER -->  
  <!-- DELIVERY TIME -->
    <class name="curam.util.internal.workflow.metadata.impl.DeliveryTime">
      <map-to xml="delivery-time"/>
      <field name="deliveryTime" get-method="getDeliveryTimeAsString" set-method="setDeliveryTimeFromString">
        <bind-xml name="seconds" node="element"/>
      </field>
      <field name="mappedDeliveryTime">
        <bind-xml name="wdo-attribute" node="element" location="mapped-delivery-time"/>
      </field>
    </class>
  <!-- DELIVERY TIME-->
  <!-- MULTIPLE OCCURRING EVENT -->
  <class name="curam.util.internal.workflow.metadata.impl.MultipleOccurringEvent">
    <map-to xml="multiple-occurring-event"/>
    <field name="listWDOName">
      <bind-xml name="list-wdo-name" node="element"/>
    </field>
  </class>
  <!-- END MULTIPLE OCCURRING EVENT -->
  <!-- MESSAGE -->
  <class name="curam.util.internal.workflow.metadata.impl.Message">
    <field name="messageText">
      <bind-xml name="message-text" node="element"/>
    </field>
    <field name="parameters" collection="arraylist" set-method="setParameters" get-method="getParameters" type="curam.util.internal.workflow.metadata.impl.QualifiedWDOAttributeName">
      <bind-xml name="wdo-attribute" node="element" location="message-parameters"/>
    </field>
  </class>
  <!-- END MESSAGE -->
  <!-- DEADLINE -->
  <class name="curam.util.internal.workflow.metadata.impl.Deadline">
    <field name="deadlineDuration">
      <bind-xml name="duration" node="element"/>
    </field>
    <field name="deadlineHandler">
      <bind-xml name="deadline-handler" node="element"/>
    </field>
     <field name="completeActivity">
        <bind-xml name="complete-activity" node="attribute"/>
    </field>
    <field name="deadlineOutputMappings">
            <bind-xml name="deadline-output-mappings" node="element"/>
    </field>
  </class>
  <!-- END DEADLINE -->
  <!-- DEADLINE HANDLER -->
  <class name="curam.util.internal.workflow.metadata.impl.DeadlineHandler">
    <field name="formalParameters" collection="arraylist"
           type="curam.util.internal.workflow.metadata.impl.FormalParameter">
      <bind-xml name="formal-parameter" node="element"
                location="formal-parameters"/>
    </field>
    <field name="interfaceName">
      <bind-xml name="interface-name" node="attribute"/>
    </field>
    <field name="methodName">
      <bind-xml name="method-name" node="attribute"/>
    </field>
  </class>
  <!-- END DEADLINE HANDLER -->
  <!-- DEADLINE DURATION -->
  <class name="curam.util.internal.workflow.metadata.impl.DeadlineDuration">
    <field name="duration" get-method="getDurationAsString" set-method="setDurationFromString">
      <bind-xml name="seconds" node="element"/>
    </field>
    <field name="mappedDuration">
      <bind-xml name="wdo-attribute" node="element" location="mapped-duration"/>
    </field>
  </class>
  <!-- DEADLINE DURATION-->
   <!-- DEADLINE OUTPUT MAPPINGS -->
   <class name="curam.util.internal.workflow.metadata.impl.DeadlineOutputMappings">
     <field name="durationExpired">
       <bind-xml name="duration-expired" node="element"/>
     </field>      
     <field name="deadlineExpiryTime">
       <bind-xml name="deadline-expiry-time" node="element"/>
     </field>            
  </class>
   <!-- DEADLINE OUTPUT MAPPINGS-->
  <!-- SUBFLOW ACTIVITY -->
  <class name="curam.util.internal.workflow.metadata.impl.SubflowActivity">
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="name"/>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="notification">
      <bind-xml name="notification" node="element"/>
    </field>
    <field name="joinType">
      <bind-xml name="join" node="element"/>
    </field>
    <field name="splitType">
      <bind-xml name="split" node="element"/>
    </field>
    <field name="subflow">
      <bind-xml name="subflow" node="element"/>
    </field>
    <field name="inputMappings" set-method="setSubflowInputMappings" get-method="getSubflowInputMappings">
      <bind-xml name="input-mappings" node="element"/>
    </field>
    <field name="outputMappings" set-method="setSubflowOutputMappings" get-method="getSubflowOutputMappings">
       <bind-xml name="output-mappings" node="element"/>
    </field>
  </class>
  <!-- END SUBFLOW ACTIVITY -->
  <!-- ROUTE ACTIVITY -->
  <class name="curam.util.internal.workflow.metadata.impl.RouteActivity">
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="name"/>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="notification">
      <bind-xml name="notification" node="element"/>
    </field>
    <field name="joinType">
      <bind-xml name="join" node="element"/>
    </field>
    <field name="splitType">
      <bind-xml name="split" node="element"/>
    </field>
  </class>
  <!-- END ROUTE ACTIVITY -->
  <!-- LOOP BEGIN ACTIVITY -->
  <class name="curam.util.internal.workflow.metadata.impl.LoopBeginActivity">
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="name"/>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="notification">
      <bind-xml name="notification" node="element"/>
    </field>
    <field name="joinType">
      <bind-xml name="join" node="element"/>
    </field>
    <field name="splitType">
      <bind-xml name="split" node="element"/>
    </field>
    <field name="loopType">
      <bind-xml name="loop-type" node="element"/>
    </field>
    <field name="condition">
      <bind-xml name="condition" node="element"/>
    </field>
    <field name="blockEndpointRef">
      <bind-xml name="block-endpoint-ref" node="element"/>
    </field>
  </class>
  <!-- END LOOP BEGIN ACTIVITY -->
  <!-- LOOP END ACTIVITY -->
  <class name="curam.util.internal.workflow.metadata.impl.LoopEndActivity">
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="name"/>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="notification">
      <bind-xml name="notification" node="element"/>
    </field>
    <field name="joinType">
      <bind-xml name="join" node="element"/>
    </field>
    <field name="splitType">
      <bind-xml name="split" node="element"/>
    </field>
    <field name="blockEndpointRef">
      <bind-xml name="block-endpoint-ref" node="element"/>
    </field>
  </class>
  <!-- END LOOP END ACTIVITY -->
  <!-- EVENT WAIT ACTIVITY -->
  <class name="curam.util.internal.workflow.metadata.impl.EventWaitActivity">
    <field name="identifier">
      <bind-xml name="id" node="attribute"/>
    </field>
    <field name="category">
      <bind-xml name="category" node="attribute"/>
    </field>
    <field name="name"/>
    <field name="description">
      <bind-xml name="description" node="element"/>
    </field>
    <field name="notification">
      <bind-xml name="notification" node="element"/>
    </field>
