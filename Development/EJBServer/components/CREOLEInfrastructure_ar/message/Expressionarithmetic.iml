<messages package="curam.creole.message">
  <message name="INVALID_NEGATIVE_DECIMAL_PLACES">
    <!--<locale language="en">The value of 'decimalPlaces' '%1s' cannot be negative.</locale> -->
    <locale language="ar">لا يمكن أن تكون قيمة '%1s' '%2s' سالبة.</locale>
  </message>
  <message name="ROUNDING_MUST_BE_SPECIFIED">
    <!--<locale language="en">If 'decimalPlaces' is specified, then 'rounding' must be specified as well.</locale> -->
    <locale language="ar">اذا تم تحديد '%1s'، فانه يجب تحديد '%2s' أيضا.</locale>
  </message>
  <message name="DECIMAL_PLACES_MUST_BE_SPECIFIED">
    <!--<locale language="en">If 'rounding' is specified, then 'decimalPlaces' must be specified as well.</locale> -->
    <locale language="ar">اذا تم تحديد '%1s'، فانه يجب تحديد '%2s' أيضا.</locale>
  </message>
  <message name="DECIMAL_PLACES_AND_ROUNDING_SHOULD_BE_SPECIFIED_FOR_DIVISION_OPERATIONS">
    <!--<locale language="en">Divsion operations should specify a number of 'decimalPlaces' and a 'rounding' mode.  If these are not specified, a runtime error will occur if no exact division calculation can be performed.</locale> -->
    <locale language="ar">يجب أن تقوم عمليات القسمة بتحديد الرقم '%1s' ونمط '%2s'.  اذا لم يتم تحديد هذا، سيحدث خطأ في التشغيل اذا لم يمكن تنفيذ أي عملية حسابية للقسمة.</locale>
  </message>
  <message name="UNSUPPORTED_OPERATION">
    <locale language="ar">عملية غير مدعمة:\t'%1s'</locale>
  </message>
</messages>

