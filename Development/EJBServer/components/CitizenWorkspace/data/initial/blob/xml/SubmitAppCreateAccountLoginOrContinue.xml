<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="page-schema.xsd">
    <identifier name="SubmitAppCreateAccountLoginOrContinue" />
    <server alias="BACK_BUTTON" method="getBackButtonVisibility">
        <param name="key$playerID" path="id" />
        <param name="key$pageName" value="SubmitAppCreateAccountLoginOrContinue" />
    </server>
    <header>
        <title id="Page.Title" />
        <description id="Page.Description" />
        <icon id="title_getting_started" />
    </header>
    <content>
        <container>
            <title id="RadioSelection.Title" />
            <help id="RadioSelection.Help" />
            <radiobutton-container id="SELECTION">
                <radiobutton id="CREATE_ACC">
                    <title id="CreateAccount.Title" />
                </radiobutton>
                <radiobutton id="LOGIN">
                    <title id="Login.Title" />
                </radiobutton>
                <radiobutton id="CONTINUE">
                    <title id="Continue.Title" />
                </radiobutton>
            </radiobutton-container>
        </container>
    </content>
    <navigation> 
        <nav-button name="back" page="SaveSubmitOrExit" conditionalSource="/BACK_BUTTON/xmlData/visible"/>
        <nav-button name="next" type="action" />
    </navigation>
</page>
