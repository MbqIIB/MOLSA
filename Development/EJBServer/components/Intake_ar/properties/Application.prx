  <property name="curam.application.applicationtransfer" dynamic="yes" constant="ENV_APPLICATIONS_TRANSFER_APPLICATION_ENABLED">
    <type>STRING</type>
    <value>YES</value>
    <default-value>YES</default-value>
    <category>APP</category>
    <locales>
      <locale language="ar">
        <display-name>يتم اتاحة نقل الطلبات</display-name>
        <description>يحدد ما اذا كان يتم اتاحة نقل الطلبات أم لا.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.application.applicationadd" dynamic="yes" constant="ENV_APPLICATIONS_ADD_APPLICATION_ENABLED">
    <type>STRING</type>
    <value>YES</value>
    <default-value>YES</default-value>
    <category>APP</category>
    <locales>
      <locale language="ar">
        <display-name>يتم اتاحة اضافة الطلبات</display-name>
        <description>يحدد ما اذا كان سيتم اتاحة اضافة الطلبات.</description>
      </locale>
    </locales>
