  <property name="curam.applicationsearch.applicationsearch" dynamic="yes" constant="ENV_APPLICATIONS_SEARCH_ENABLED">
    <type>STRING</type>
    <value>YES</value>
    <default-value>YES</default-value>
    <category>APPSEARCH</category>
    <locales>
      <locale language="ar">
        <display-name>محددات بحث الطلب</display-name>
        <description>ما اذا كانت الطلبات يجب أن يتم تضمينها في بحث الطلبات أم لا.</description>
      </locale>
    </locales>
  </property>
  <property name="curam.applicationsearch.myapplicationsearch" dynamic="yes" constant="ENV_MY_APPLICATIONS_SEARCH_MAX_ROWS">
    <type>STRING</type>
    <value>1000</value>
    <default-value>1000</default-value>
    <category>APPSEARCH</category>
    <locales>
      <locale language="ar">
        <display-name>الحد الأقصى لعدد الصفوف التي يتم ارجاعها من البياني الديناميكي</display-name>
