<messages package="curam.message">
  <message name="ERR_SS_BAD_QUERY">
    <locale language="ar">تم اكتشاف استعلام غير صحيح.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_NO_TERMS">
    <locale language="ar">لا يحتوي الاستعلام على أية مصطلحات - لا يعد استعلام صحيح.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_NO_VALID_TERMS">
    <locale language="ar">لا يحتوي الاستعلام على أية مصطلحات صحيحة - لا يعد استعلام صحيح.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_UNKNOWN_FIELD">
    <locale language="ar">يحتوي الاستعلام على مصطلحات تشير الى مجال بالاسم \'%1s\' وهو غير معروف أو لم يتم تخزينه في خدمة البحث هذه.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_UNINDEXED_FIELD">
    <locale language="ar">يحتوي الاستعلام على مصطلحات تشير الى مجال بالاسم \'%1s\' لم يتم فهرسته في خدمة البحث هذه.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_NO_FIELDS">
    <locale language="ar">لا يحتوي الاستعلام على أية مجالات لارجاعها - لا يعد استعلام صحيح.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_UNKNOWN_RETURN_FIELD">
    <locale language="ar">يقوم الاستعلام بمحاولة ارجاع مجال غير معروف \'%1s\'.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_UNTOKENIZED_WILDCARD">
    <locale language="ar">لا يتم دعم استعلامات الحرف الشامل غير محددة الرمز المميز.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_EMPTY_TERM">
    <locale language="ar">تم اكتشاف مصطلح خالي.</locale>
  </message>
  <message name="ERR_SS_BAD_QUERY_MULTIPLE_TERM_TOKEN_FIELD">
    <locale language="ar">تم اكتشاف استعلام ذو بادئة مع عدة رموز مميزة في مجال محدد الرمز المميز.</locale>
  </message>
</messages>

