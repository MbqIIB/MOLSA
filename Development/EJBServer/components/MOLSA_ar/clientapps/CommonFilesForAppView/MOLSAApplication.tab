<?xml version="1.0" encoding="UTF-8"?>

<tc:tab-config xmlns:tc="http://www.curamsoftware.com/curam/util/client/tab-config"
  id="MOLSAApplication">

  <tc:page-param name="applicationID"/>

  <tc:menu id="MOLSAApplicationMenu"/>

  <tc:context tab-name="Details.Name.Application" tab-title="Details.Title.Application"
    page-id="ApplicationContextPanel" height="155"/>

  <tc:smart-panel page-id="SmartPanelHome" title="Smart Panel" collapsed="true" width="250"/>

  <tc:navigation id="MOLSAApplication"/>


  <!-- Tab on refresh -->
  <tc:tab-refresh>
    <tc:onsubmit page-id="Application_readyForDetermination" context="true" menu-bar="true"/>
    <tc:onload page-id="ProgramRecommendation_listProgramRecommendationsForApplication1"
      context="true" menu-bar="true"/>
    <tc:onsubmit context="true" menu-bar="true" navigation="true"
      page-id="Intake_confirmProgramWithdrawalRequest"/>
    <tc:onsubmit context="true" menu-bar="true" navigation="true"
      page-id="Intake_createProgramWithdrawalRequest"/>
    <tc:onsubmit context="true" menu-bar="true" navigation="true"
      page-id="Application_modifyOwner"/>
    <tc:onsubmit page-id="Intake_addProgramApplication" context="true"/>  
    <tc:onsubmit context="true" menu-bar="true" navigation="true"
            page-id="Application_confirmChangeOwnerToMe"/>
  </tc:tab-refresh>

</tc:tab-config>
