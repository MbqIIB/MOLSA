<RuleSet name="AdditionalBenefitRuleSet">
    <Class extends="DynamicEvidence" extendsRuleSet="DynamicEvidenceRuleSet"
        name="AdditionalBenefit">
        <Attribute name="amount">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Number"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="benefitType">
            <Annotations/>
            <type>
                <codetableentry table="MOLSABenefitType"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="benefitProvider">
            <Annotations/>
            <type>
                <javaclass name="java.lang.String"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="date">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="participant">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Number"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="related_participant">
            <Annotations/>
            <type>
                <ruleclass name="CaseParticipantRole" ruleset="CaseEntitiesRuleSet"/>
            </type>
            <derivation>
                <reference attribute="caseParticipantRoleRecord">
                    <create ruleclass="CaseParticipantRoleCalculator"
                        ruleset="DynamicEvidenceRuleSet">
                        <reference attribute="participant"/>
                    </create>
                </reference>
            </derivation>
        </Attribute>
    </Class>
</RuleSet>
