<?xml version="1.0" encoding="UTF-8"?>
<!--
  Application Development script that wraps the server build.xml file. This file is not shipped to customers and shouldn't
  contain any scripts that customers may depend on.
-->
<project name="DependencyManagerBatchProperties">
  <property environment="env"/>


  <property
    name="java.failonerror"
    value="false"
  />


  <property
    name="build.logs.basedir"
    value="${env.SERVER_DIR}/buildlogs"
  />


  <property
    name="dir.logs.dependencymanager"
    value="./Logs"
  />
  <property
    name="dir.logs.dependencymanager.detailed"
    value="${dir.logs.dependencymanager}/DetailedLogs"
  />


  <property
    name="summary.file.postfix"
    value="_Summary"
  />
  <property
    name="dependencymanager.error.file"
    value="${dir.logs.dependencymanager}/PropagationErrors.log"
  />


  <property
    name="submit.precedent.change.set.log"
    value="SubmitPrecedentChangeSet"
  />
  <property
    name="perform.batch.recalculations.stream.log"
    value="PerformBatchRecalculations_Stream"
  />
  <property
    name="perform.batch.recalculations.chunker.log"
    value="PerformBatchRecalculations_Chunker"
  />
  <property
    name="complete.precedent.change.set.log"
    value="CompletePrecedentChangeSet"
  />


</project>
