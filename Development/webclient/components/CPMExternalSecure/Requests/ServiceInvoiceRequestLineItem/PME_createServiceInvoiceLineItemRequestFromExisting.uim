  <PAGE_PARAMETER NAME="serviceInvoiceRequestLineItemID"/>
  <PAGE_PARAMETER NAME="pageContextDescription"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="serviceInvoiceRequestLineItemID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$serviceInvReqLineItemID"
    />
  </CONNECT>


