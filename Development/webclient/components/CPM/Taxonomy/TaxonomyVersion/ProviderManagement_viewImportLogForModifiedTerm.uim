<!-- Description                                                             -->
<!-- ===========                                                             -->
<!-- This page allows the user to view the change details of the modified    -->
<!-- term in import log.                                                     -->
<PAGE
  PAGE_ID="ProviderManagement_viewImportLogForModifiedTerm"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="taxonomyImportLogID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="taxonomyImportLogID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$taxonomyImportLogID"
    />
  </CONNECT>


  <SERVER_INTERFACE
    CLASS="MaintainTaxonomyImportLog"
    NAME="DISPLAY"
    OPERATION="viewTaxonomyImportLog"
    PHASE="DISPLAY"
  />


  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="2"
    TITLE="Cluster.Title.TermDetails"
  >


    <FIELD LABEL="Field.Label.NewCode">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="newCode"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.CreatedDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="createdDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.NewName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="newName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <CLUSTER LABEL_WIDTH="30">


    <FIELD LABEL="Field.Label.NewDefinition">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="newDefinition"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.OldDefinition">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="oldDefinition"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Description">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="description"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
