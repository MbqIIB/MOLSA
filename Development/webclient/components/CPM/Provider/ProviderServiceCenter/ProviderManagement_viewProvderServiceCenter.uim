          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$providerServiceCenterSummaryDetails$serviceCenterProviderOfferingDetails$servCenterProviderOfferingID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="servCenterProviderOfferingID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$providerServiceCenterSummaryDetails$serviceCenterProviderOfferingDetails$versionNo"
