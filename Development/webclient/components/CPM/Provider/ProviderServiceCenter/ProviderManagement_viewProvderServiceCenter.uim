      PROPERTY="providerServiceCenterID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$providerServiceCenterKey$providerServiceCenterID"
    />
  </CONNECT>


  <!-- BEGIN, CR00246961, GP -->
  <CLUSTER
    LABEL_WIDTH="35"
    NUM_COLS="2"
    TITLE="Cluster.Address.Label"
  >
    <!-- END, CR00246961 -->
    <FIELD LABEL="Field.Label.Address">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerServiceCenterSummaryDetails$contactDetails$formattedAddress"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EmailAddress">
