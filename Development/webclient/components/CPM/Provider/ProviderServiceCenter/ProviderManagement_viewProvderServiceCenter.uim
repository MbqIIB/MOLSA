      <FIELD HEIGHT="4">
        <!-- END, CR00207545 -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$clientInformation"
          />
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="result$clientInfoExistsInd"
          />
        </CONDITION>
        <!-- BEGIN, CR00206671, SS -->
        <!-- BEGIN, CR00187417, PS -->
        <LINK
          DISMISS_MODAL="true"
          OPEN_MODAL="true"
          PAGE_ID="ProviderManagement_addClientInfoTranslationForServiceCenter"
          WINDOW_OPTIONS="width=350"
        >
          <!-- END, CR00187417 -->
          <!-- END, CR00206671 -->
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="providerServiceCenterID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="providerServiceCenterID"
            />
          </CONNECT>


          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$providerServiceCenterSummaryDetails$providerServiceCenterDtls$versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="clientInfoAddInd"
          />
        </CONDITION>
        <!-- BEGIN, CR00206671, SS -->
        <!-- BEGIN, CR00187417, PS -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ProviderManagement_addLocalizableTextTranslation"
          WINDOW_OPTIONS="width=350"
        >
          <!-- END, CR00187417 -->
          <!-- END, CR00206671 -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$providerServiceCenterSummaryDetails$providerServiceCenterDtls$clientInfoTextID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="clientInfoViewInd"
          />
        </CONDITION>
        <!-- BEGIN, CR00187417, PS -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ProviderManagement_viewLocalizableTextTranslation"
          WINDOW_OPTIONS="width=700"
        >
          <!-- END, CR00187417 -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$providerServiceCenterSummaryDetails$providerServiceCenterDtls$clientInfoTextID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


