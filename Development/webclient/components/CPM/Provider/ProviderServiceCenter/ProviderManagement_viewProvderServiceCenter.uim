    LABEL_WIDTH="45"
    NUM_COLS="2"
    TITLE="Cluster.Title.AdditionalInformation"
  >
    <!-- END, CR00228396 -->
    <CONTAINER LABEL="Field.Label.AreasServedInfo">
      <FIELD HEIGHT="4">
        <!-- END, CR00207545 -->
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$areasServedInformation"
          />
        </CONNECT>
      </FIELD>
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <CONDITION>
          <IS_FALSE
            NAME="DISPLAY"
            PROPERTY="result$areasServedInfoExistsInd"
          />
        </CONDITION>
        <!-- BEGIN, CR00206671, SS -->
        <!-- BEGIN, CR00187417, PS -->
        <LINK
          DISMISS_MODAL="true"
          OPEN_MODAL="true"
          PAGE_ID="ProviderManagement_addAreasServedInfoTranslationForServiceCenter"
          WINDOW_OPTIONS="width=350"
        >
          <!-- END, CR00187417 -->
          <!-- END, CR00206671 -->
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="providerServiceCenterID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="providerServiceCenterID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="pageContextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageContextDescription"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$providerServiceCenterSummaryDetails$providerServiceCenterDtls$versionNo"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="versionNo"
            />
          </CONNECT>


        </LINK>
      </ACTION_CONTROL>
      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="areaServiceAddInd"
          />
        </CONDITION>
        <!-- BEGIN, CR00206671, SS -->
        <!-- BEGIN, CR00187417, PS -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ProviderManagement_addLocalizableTextTranslation"
          WINDOW_OPTIONS="width=350"
        >
          <!-- END, CR00187417 -->
          <!-- END, CR00206671 -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$providerServiceCenterSummaryDetails$providerServiceCenterDtls$areasSvdInfoTxtID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>


      <ACTION_CONTROL
        IMAGE="LocalizableTextTranslation"
        LABEL="ActionControl.Label.TextTranslation"
      >
        <CONDITION>
          <IS_TRUE
            NAME="DISPLAY"
            PROPERTY="areaServiceViewInd"
          />
        </CONDITION>
        <!-- BEGIN, CR00187417, PS -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ProviderManagement_viewLocalizableTextTranslation"
          WINDOW_OPTIONS="width=700"
        >
          <!-- END, CR00187417 -->
          <CONNECT>
            <SOURCE
              NAME="DISPLAY"
              PROPERTY="result$providerServiceCenterSummaryDetails$providerServiceCenterDtls$areasSvdInfoTxtID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="localizableTextID"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
