<!-- This page allows the user to view details of third party deduction.    -->
<PAGE
  PAGE_ID="ProviderManagement_viewThirdPartyDeductionDetails"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="providerDeductionID"/>
  <PAGE_PARAMETER NAME="pageContextDescription"/>


  <SERVER_INTERFACE
    CLASS="MaintainProviderDeduction"
    NAME="DISPLAY"
    OPERATION="viewProviderDeduction"
  />


  <MENU MODE="IN_PAGE_NAVIGATION">
    <ACTION_CONTROL
      LABEL="Menu.ViewThirdPartyDeduction"
      STYLE="in-page-current-link"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="ProviderManagement_viewThirdPartyDeductionDetails">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="providerDeductionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="providerDeductionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageContextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageContextDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <ACTION_CONTROL
      LABEL="Menu.ViewDeductionHistory"
      STYLE="in-page-link"
      TYPE="ACTION"
    >
      <LINK PAGE_ID="ProviderManagement_viewThirdPartyProviderDeductionHistory">
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="providerDeductionID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="providerDeductionID"
          />
        </CONNECT>
        <CONNECT>
          <SOURCE
            NAME="PAGE"
            PROPERTY="pageContextDescription"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="pageContextDescription"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
  </MENU>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="providerDeductionID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="key$providerDeductionID"
    />
  </CONNECT>


  <!-- BEGIN, CR00228396, PS -->
  <CLUSTER NUM_COLS="2">
    <!-- END, CR00228396 -->
    <FIELD LABEL="Field.Label.Name">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$deductionName"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.StartDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$startDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Percentage">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$rate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AccountName">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$customerAccName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.DateCreated">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$createdDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ActionType">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$actionType"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ThirdParty">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$liabilityConcernRoleName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.EndDate">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$endDate"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Priority">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$priority"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.AccountNumber">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$customerAccNumber"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.BusinessStatus">
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$businessStatus"
        />
      </CONNECT>
    </FIELD>


    <CONTAINER LABEL="Container.Label.PaymentType">
      <FIELD>
        <CONNECT>
          <SOURCE
            NAME="DISPLAY"
            PROPERTY="result$paymentType"
          />
        </CONNECT>
      </FIELD>
      <!-- BEGIN, CR00228396, PS -->
      <ACTION_CONTROL
        APPEND_ELLIPSIS="false"
        LABEL="ActionControl.Label.Change"
      >
        <!-- END, CR00228396 -->
        <LINK
          OPEN_MODAL="true"
          PAGE_ID="ProviderManagement_modifyPaymentType"
        >
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="providerDeductionID"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="providerDeductionID"
            />
          </CONNECT>
          <CONNECT>
            <SOURCE
              NAME="PAGE"
              PROPERTY="pageContextDescription"
            />
            <TARGET
              NAME="PAGE"
              PROPERTY="pageContextDescription"
            />
          </CONNECT>
        </LINK>
      </ACTION_CONTROL>
    </CONTAINER>


  </CLUSTER>
  <CLUSTER
    SHOW_LABELS="false"
    TITLE="Cluster.Title.Comments"
  >
    <!-- BEGIN, CR00417165, GK -->
    <FIELD
      HEIGHT="3"
      LABEL="Field.Label.Comments"
    >
      <!-- END, CR00417165 -->
      <CONNECT>
        <SOURCE
          NAME="DISPLAY"
          PROPERTY="result$providerDeduction$comments"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
