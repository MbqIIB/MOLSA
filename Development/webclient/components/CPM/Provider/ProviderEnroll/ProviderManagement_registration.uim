                <LINK PAGE_ID="ProviderManagement_searchConfirmProviderNotRegistered"/>
            </ACTION_CONTROL>
        </CONTAINER>
        <CONTAINER>
            <ACTION_CONTROL LABEL="ActionControl.Label.RegisterProviderGroup">
                <LINK PAGE_ID="ProviderManagement_searchConfirmProviderGroupNotRegistered"/>
