        </CONTAINER>
        <CONTAINER>
            <ACTION_CONTROL LABEL="ActionControl.Label.CreateEnquiry">
                <LINK PAGE_ID="ProviderManagement_searchOpenEnquiry"/>
            </ACTION_CONTROL>
        </CONTAINER>
        <CONTAINER>
            <ACTION_CONTROL LABEL="ActionControl.Label.ExitRegistration">
                <LINK PAGE_ID="ProviderManagement_resourceMgrHome"/>
            </ACTION_CONTROL>
        </CONTAINER>
     </CLUSTER>
    
</PAGE>