<!-- Description -->
<!-- =========== -->
<!-- This page allows the user to add client to service invoice line item       -->
<!--  correction page.                                                          -->
<PAGE
  PAGE_ID="ProviderManagement_addClientToSILICorrection"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="SILICorrection"
    NAME="ACTION"
    OPERATION="addClientToServiceInvoiceLineItemCorrection"
    PHASE="ACTION"
  />


  <PAGE_PARAMETER NAME="siliCorrectionID"/>
  <PAGE_PARAMETER NAME="siliReferenceNo"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="siliCorrectionID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="details$siliCorrectionID"
    />
  </CONNECT>


  <CLUSTER
    LABEL_WIDTH="40"
    NUM_COLS="2"
    TAB_ORDER="ROW"
  >
    <FIELD LABEL="Field.Label.ClientReferenceNumber">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$clientReferenceNo"
        />
      </CONNECT>
    </FIELD>
    <FIELD
      LABEL="Field.Label.ClientDateofBirth"
      USE_BLANK="true"
      USE_DEFAULT="false"
    >
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$clientDOB"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ClientFirstName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$clientFirstName"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.ClientLastName">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$clientLastName"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.SaveAndNew"
      TYPE="SUBMIT"
    >
      <LINK PAGE_ID="ProviderManagement_addClientToSILICorrection"/>
    </ACTION_CONTROL>


    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >

    </ACTION_CONTROL>


    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel">
    </ACTION_CONTROL>
  </ACTION_SET>
</PAGE>
