<!-- This is a create Training Requirement for Service Offering Page        -->
<PAGE
  PAGE_ID="ProviderManagement_createTrainingRequirementForServiceOffering"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
  WINDOW_OPTIONS="width=400"
>
  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.StaticText1"
      />
    </CONNECT>
  </PAGE_TITLE>


  <PAGE_PARAMETER NAME="serviceOfferingName"/>
  <PAGE_PARAMETER NAME="serviceOfferingID"/>


  <SERVER_INTERFACE
    CLASS="ServiceOffering"
    NAME="ACTION"
    OPERATION="createTrainingRequirementForServiceOffering"
    PHASE="ACTION"
  />
  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="serviceOfferingID"
    />
    <TARGET
      NAME="ACTION"
      PROPERTY="serviceOfferingID"
    />
  </CONNECT>


  <ACTION_SET ALIGNMENT="CENTER">
    <!-- BEGIN, CR00228079, PS -->
    <ACTION_CONTROL
      LABEL="ActionControl.Label.SaveAndAdd"
      TYPE="SUBMIT"
      >
    </ACTION_CONTROL>
    <!-- END, CR00228079 -->
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      
    </ACTION_CONTROL>
    
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


  <!-- BEGIN, CR00197208, PS -->
  <!-- BEGIN, CR00228079, PS -->
  <CLUSTER
    LABEL_WIDTH="30"
    NUM_COLS="1"
  >
    <!-- END, CR00228079 -->
    <!-- END, CR00197208 -->
    <FIELD LABEL="Field.Label.TrainingRequirement.Training">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$dtls$trainingID"
        />
      </CONNECT>
    </FIELD>
    <!-- BEGIN, CR00197766, PS -->
    <FIELD
      LABEL="Field.Label.TrainingRequirement.Completion"
      WIDTH="55"
    >
      <!-- END, CR00197766 -->
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="details$dtls$completion"
        />
      </CONNECT>
    </FIELD>
  </CLUSTER>
</PAGE>
