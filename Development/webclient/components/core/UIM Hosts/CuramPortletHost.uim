  <PAGE_PARAMETER NAME="podID"/>


  <CONNECT>
    <SOURCE
      NAME="PAGE"
      PROPERTY="podID"
    />
    <TARGET
      NAME="DISPLAY"
      PROPERTY="pageID$pageID"
    />
  </CONNECT>


  <INCLUDE FILE_NAME="PodContainer.vim"/>


</PAGE>
