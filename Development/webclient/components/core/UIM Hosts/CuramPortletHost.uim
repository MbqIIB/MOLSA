<PAGE
  COMPONENT_STYLE="page-omit-sidebar"
  PAGE_ID="CuramPortletHost"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
