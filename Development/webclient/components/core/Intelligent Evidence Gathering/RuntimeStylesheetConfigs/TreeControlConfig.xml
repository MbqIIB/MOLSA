<nc:node-config xmlns:nc="http://www.curamsoftware.com/curam/jde/client/widget/nc">


  <nc:node-info name="StandardIEGScriptTree">


    <nc:node-types>
      <nc:node-type icon="../Images/script_g16x16.gif" name="script">
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="IEG_ViewQuestionScript">
              <nc:set-page-param from-key="scriptID" name="questionScriptIDParam"/>
            </nc:open-page-action>
          </nc:action>
        </nc:actions>
      </nc:node-type>

      <nc:node-type icon="../Images/script_g16x16.gif" name="subScript">
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="IEG_ViewQuestionSubScript">
              <nc:set-page-param from-key="questionSubScriptID" name="questionSubScriptIDParam"/>
              <nc:set-page-param from-key="scriptID" name="questionScriptIDParam"/>
              <nc:set-page-param from-key="questionPageID" name="questionPageIDParam"/>
            </nc:open-page-action>
          </nc:action>
        </nc:actions>
      </nc:node-type>      
      
      <nc:node-type icon="../Images/page_g16x16.gif" name="question-page">
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="IEG_ViewQuestionPage">
              <nc:set-page-param from-key="questionPageID" name="questionPageIDParam"/>
              <nc:set-page-param from-key="scriptID" name="questionScriptIDParam"/>
            </nc:open-page-action>
          </nc:action>
        </nc:actions>
      </nc:node-type>
      
      <nc:node-type icon="../Images/precondition_g16x16.gif" name="precondition">
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="IEG_ViewPrecondition">
              <nc:set-page-param from-key="questionPageID" name="questionPageIDParam"/>
              <nc:set-page-param from-key="scriptID" name="questionScriptIDParam"/>
            </nc:open-page-action>
          </nc:action>
        </nc:actions>
      </nc:node-type>
      
      <nc:node-type icon="../Images/postcondition_g16x16.gif" name="postcondition">
        <nc:actions>
	  <nc:action name="view">
	    <nc:open-page-action page-id="IEG_ViewPostcondition">
	      <nc:set-page-param from-key="questionPageID" name="questionPageIDParam"/>
	      <nc:set-page-param from-key="scriptID" name="questionScriptIDParam"/>
	      <nc:set-page-param from-key="postconditionID" name="postconditionIDParam"/>
	    </nc:open-page-action>
	  </nc:action>
        </nc:actions>
      </nc:node-type>
      
      <nc:node-type icon="../Images/questiongroup_g16x16.gif" name="question-group">
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="IEG_resolveViewQuestionGroup">
              <nc:set-page-param from-key="scriptID" name="questionScriptIDParam"/>
              <nc:set-page-param from-key="questionPageID" name="questionPageIDParam"/>
              <nc:set-page-param from-key="questionGroupID" name="questionGroupIDParam"/>
            </nc:open-page-action>
          </nc:action>
        </nc:actions>
      </nc:node-type>
      
      <nc:node-type icon="../Images/question_g16x16.gif" name="question">
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="IEG_ViewQuestion">
              <nc:set-page-param from-key="scriptID" name="questionScriptIDParam"/>
              <nc:set-page-param from-key="questionPageID" name="questionPageIDParam"/>
              <nc:set-page-param from-key="questionGroupID" name="questionGroupIDParam"/>
              <nc:set-page-param from-key="questionID" name="questionIDParam"/>
            </nc:open-page-action>
          </nc:action>
        </nc:actions>
      </nc:node-type>
      
      <nc:node-type icon="../Images/label_g16x16.gif" name="hyperlink-label">
        <nc:actions>
          <nc:action name="view">
            <nc:open-page-action page-id="IEG_ViewHyperlinkLabel">
              <nc:set-page-param from-key="scriptID" name="questionScriptIDParam"/>
              <nc:set-page-param from-key="questionPageID" name="questionPageIDParam"/>
              <nc:set-page-param from-key="questionGroupID" name="questionGroupIDParam"/>
              <nc:set-page-param from-key="hyperlinkLabelID" name="hyperlinkLabelIDParam"/>
            </nc:open-page-action>
          </nc:action>
        </nc:actions>
      </nc:node-type>
      
    </nc:node-types>

  </nc:node-info>


</nc:node-config>