<!-- Configuration file for the web client application.                         -->
<APP_CONFIG>
  <ERROR_PAGE PAGE_ID="Application_viewError"/>
  <MULTIPLE_SELECT>
    <DOMAIN
      MULTIPLE="true"
      NAME="INTAKE_BIG_STRING"
    />
    <DOMAIN
      MULTIPLE="true"
      NAME="APPLICATION_CODE_TAB_LIST"
    />
  </MULTIPLE_SELECT>


  <FILE_DOWNLOAD_CONFIG>


    <!-- BEGIN, CR00267256, JS -->
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IntakeProcessAdminPkg.IntakeProcessAdmin_getProcessConfigurationXML_TH"
      PAGE_ID="IntakeAdmin_modifyProcessConfiguration"
    >
      <INPUT
        PAGE_PARAM="processID"
        PROPERTY="key$processID"
      />
      <FILE_NAME PROPERTY="result$fileName"/>
      <FILE_DATA PROPERTY="result$processXML"/>
      <CONTENT_TYPE PROPERTY="result$contentType"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IntakeProcessAdminPkg.IntakeProcessAdmin_getProcessConfigurationXML_TH"
      PAGE_ID="IntakeAdmin_listPublishedProcesses"
    >
      <INPUT
        PAGE_PARAM="processID"
        PROPERTY="key$processID"
      />
      <FILE_NAME PROPERTY="result$fileName"/>
      <FILE_DATA PROPERTY="result$processXML"/>
      <CONTENT_TYPE PROPERTY="result$contentType"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IntakeProcessAdminPkg.IntakeProcessAdmin_getProcessConfigurationXML_TH"
      PAGE_ID="IntakeAdmin_listUnpublishedProcesses"
    >
      <INPUT
        PAGE_PARAM="processID"
        PROPERTY="key$processID"
      />
      <FILE_NAME PROPERTY="result$fileName"/>
      <FILE_DATA PROPERTY="result$processXML"/>
      <CONTENT_TYPE PROPERTY="result$contentType"/>
    </FILE_DOWNLOAD>
    <!-- END, CR00267256 -->


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IntakeApplicationPkg.IntakeApplication_readAttachment_TH"
      PAGE_ID="IntakeCustomHome"
    >
      <INPUT
        PAGE_PARAM="attachmentID"
        PROPERTY="key$attachmentID"
      />
      <FILE_NAME PROPERTY="result$attachmentName"/>
      <FILE_DATA PROPERTY="result$attachmentContents"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IntakeScreeningPkg.IntakeScreening_readApplicationPdf_TH"
      PAGE_ID="Intake_listScreenings"
    >
      <INPUT
        PAGE_PARAM="attachmentID"
        PROPERTY="key$attachmentID"
      />
      <FILE_NAME PROPERTY="result$fileName"/>
      <FILE_DATA PROPERTY="result$fileData"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IntakeLifeEventPkg.IntakeLifeEvent_viewAnswersPDF_TH"
      PAGE_ID="Intake_listLifeEvents"
    >
      <INPUT
        PAGE_PARAM="attachmentID"
        PROPERTY="key$attachmentID"
      />
      <FILE_NAME PROPERTY="result$fileName"/>
      <FILE_DATA PROPERTY="result$fileData"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.ApplicationPkg.Application_readAttachment_TH"
      PAGE_ID="Application_viewAttachment"
    >
      <INPUT
        PAGE_PARAM="attachmentLinkID"
        PROPERTY="key$attachmentLinkID"
      />
      <FILE_NAME PROPERTY="result$attachmentDtls$attachmentName"/>
      <FILE_DATA PROPERTY="result$attachmentDtls$attachmentContents"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.ApplicationPkg.Application_readAttachment_TH"
      PAGE_ID="Application_modifyAttachmentFromList"
    >
      <INPUT
        PAGE_PARAM="attachmentLinkID"
        PROPERTY="key$attachmentLinkID"
      />
      <FILE_NAME PROPERTY="result$attachmentDtls$attachmentName"/>
      <FILE_DATA PROPERTY="result$attachmentDtls$attachmentContents"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IntakeScreeningPkg.IntakeScreening_readApplicationPdf_TH"
      PAGE_ID="Intake_listApplicationsForPerson"
    >
      <INPUT
        PAGE_PARAM="attachmentID"
        PROPERTY="key$attachmentID"
      />
      <FILE_NAME PROPERTY="result$fileName"/>
      <FILE_DATA PROPERTY="result$fileData"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.IntakeScreeningPkg.IntakeScreening_readApplicationPdf_TH"
      PAGE_ID="Intake_listApplications"
    >
      <INPUT
        PAGE_PARAM="attachmentID"
        PROPERTY="key$attachmentID"
      />
      <FILE_NAME PROPERTY="result$fileName"/>
      <FILE_DATA PROPERTY="result$fileData"/>
    </FILE_DOWNLOAD>


    <!--BEGIN, CR00283954, JD -->


    <!-- Open MS Word from the view page-->
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.ParticipantPkg.Participant_readCommunicationAttachment_TH"
      PAGE_ID="Application_viewMSWordCommunication"
    >
      <INPUT
        PAGE_PARAM="attachmentID"
        PROPERTY="key$readAttachmentIn$attachmentID"
      />
      <FILE_NAME PROPERTY="result$readCommunicationAttachmentDtls$attachmentName"/>
      <FILE_DATA PROPERTY="result$readCommunicationAttachmentDtls$attachmentContents"/>
    </FILE_DOWNLOAD>


    <!-- Open MS Word and ProForma from the Row level Action-->
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.CommunicationPkg.Communication_printCommunication_TH"
      PAGE_ID="Application_listCommunication"
    >
      <INPUT
        PAGE_PARAM="communicationID"
        PROPERTY="communicationIDKey$communicationID"
      />
      <FILE_DATA PROPERTY="result$fileContent"/>
      <FILE_NAME PROPERTY="result$fileName"/>
    </FILE_DOWNLOAD>


    <FILE_DOWNLOAD
      CLASS="curam.interfaces.CommunicationPkg.Communication_printCommunication_TH"
      PAGE_ID="Application_listCommunication1"
    >
      <INPUT
        PAGE_PARAM="communicationID"
        PROPERTY="communicationIDKey$communicationID"
      />
      <FILE_DATA PROPERTY="result$fileContent"/>
      <FILE_NAME PROPERTY="result$fileName"/>
    </FILE_DOWNLOAD>


    <!-- Open Associated Files from the Recorded Communication-->
    <FILE_DOWNLOAD
      CLASS="curam.interfaces.ParticipantPkg.Participant_readCommunicationAttachment_TH"
      PAGE_ID="Application_viewRecordedCommunication"
    >
      <INPUT
        PAGE_PARAM="attachmentID"
        PROPERTY="key$readAttachmentIn$attachmentID"
      />
      <FILE_NAME PROPERTY="result$readCommunicationAttachmentDtls$attachmentName"/>
      <FILE_DATA PROPERTY="result$readCommunicationAttachmentDtls$attachmentContents"/>
    </FILE_DOWNLOAD>


    <!--END, CR00283954-->


  </FILE_DOWNLOAD_CONFIG>


  <POPUP_PAGES>
    <POPUP_PAGE PAGE_ID="PersonProspectPerson_searchPopup">
      <DOMAIN>PERSON_PROSPECT_ID</DOMAIN>
      <CT_CODE/>
      <WIDTH>800</WIDTH>
      <HEIGHT>600</HEIGHT>
      <SCROLLBARS>true</SCROLLBARS>
      <IMAGE>Images/search.gif </IMAGE>
      <LABEL>Search</LABEL>
    </POPUP_PAGE>
  </POPUP_PAGES>
  <PAGINATION ENABLED="true">
    <DEFAULT_PAGE_SIZE>15</DEFAULT_PAGE_SIZE>
    <PAGINATION_THRESHOLD>15</PAGINATION_THRESHOLD>
  </PAGINATION>


</APP_CONFIG>
