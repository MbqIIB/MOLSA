<!-- This is a sample page used to create an emergency placement -->
<!-- BEGIN, CR00248676, FM -->
<?curam-deprecated Since Curam 6.0.
  This page is deprecated as it is not supported and is implemented for 
  internal use only. See release note: CR00248676 ?>
<!-- END, CR00248676 -->
<PAGE
  PAGE_ID="ProviderManagement_createSampleEmergencyPlacement"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="file://Curam/UIMSchema.xsd"
>


  <PAGE_TITLE>
    <CONNECT>
      <SOURCE
        NAME="TEXT"
        PROPERTY="PageTitle.Home"
      />
    </CONNECT>
  </PAGE_TITLE>


  <SERVER_INTERFACE
    CLASS="SamplePlacement"
    NAME="ACTION"
    OPERATION="createEmergencyPlacement"
    PHASE="ACTION"
  />


  <CLUSTER
    LABEL_WIDTH="30"
    TITLE="Cluster.Details.Title"
  >


    <FIELD LABEL="Field.Label.Placement.CaseParticipantRoleID">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="caseParticipantRoleId"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Placement.ServiceOfferingID">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="serviceOfferingID"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Placement.ProviderID">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="providerConcernRoleID"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Placement.FromDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="fromDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD CONTROL="SKIP"/>


    <FIELD LABEL="Field.Label.Placement.ToDate">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="toDate"
        />
      </CONNECT>
    </FIELD>


    <FIELD LABEL="Field.Label.Placement.unitAmount">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="unitAmount"
        />
      </CONNECT>
    </FIELD>
    <FIELD LABEL="Field.Label.Placement.unitsAmountFixedInd">
      <CONNECT>
        <TARGET
          NAME="ACTION"
          PROPERTY="unitsAmountFixedInd"
        />
      </CONNECT>
    </FIELD>


  </CLUSTER>


  <ACTION_SET
    ALIGNMENT="CENTER"
    TOP="false"
  >
    <ACTION_CONTROL
      LABEL="ActionControl.Label.Save"
      TYPE="SUBMIT"
    >
      <!-- BEGIN, CR00118628, RPB-->
      <LINK PAGE_ID="ProviderManagement_compartmentTreeWindow">
        <CONNECT>
          <SOURCE
            NAME="ACTION"
            PROPERTY="providerConcernRoleID"
          />
          <TARGET
            NAME="PAGE"
            PROPERTY="concernRoleID"
          />
        </CONNECT>
      </LINK>
    </ACTION_CONTROL>
    <!-- END, CR00118628 -->
    <ACTION_CONTROL LABEL="ActionControl.Label.Cancel"/>
  </ACTION_SET>


</PAGE>
