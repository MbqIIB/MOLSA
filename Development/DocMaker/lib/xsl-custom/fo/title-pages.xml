<t:templates
  xmlns:fo="http://www.w3.org/1999/XSL/Format"
  xmlns:param="http://nwalsh.com/docbook/xsl/template/1.0/param"
  xmlns:t="http://nwalsh.com/docbook/xsl/template/1.0"
  xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
>


  <!-- ==================================================================== -->


  <t:titlepage
    font-family="{$body.fontset}"
    t:element="book"
    t:wrapper="fo:block"
  >


    <t:titlepage-content t:side="recto">


      <title
        end-indent="10mm"
        font-family="{$title.fontset}"
        font-style="normal"
        font-weight="normal"
        hyphenate="false"
        padding-before="10mm"
        padding-end="10mm"
        start-indent="20mm"
        text-align="left"
      />


    </t:titlepage-content>


    <!--
    Need the page break here, as the recto/verso side designations do not
    break the page properly on their own.
    -->
    <t:titlepage-content
      break-before="page"
      t:side="verso"
    >


      <productname
        font-size="11pt"
        text-align="left"
      />


      <copyright
        font-size="11pt"
        space-before="15pt"
        text-align="left"
      />


    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <!-- ==================================================================== -->


  <t:titlepage
    font-family="{$title.fontset}"
    t:element="chapter"
    t:wrapper="fo:block"
  >
    <t:titlepage-content
      margin-left="{$title.margin.left}"
      t:side="recto"
    >
      <title
        param:node="ancestor-or-self::chapter[1]"
        t:named-template="component.title"
      />
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <!-- ==================================================================== -->


  <t:titlepage
    font-family="{$title.fontset}"
    t:element="appendix"
    t:wrapper="fo:block"
  >
    <t:titlepage-content
      margin-left="{$title.margin.left}"
      t:side="recto"
    >
      <title
        param:node="ancestor-or-self::appendix[1]"
        t:named-template="component.title"
      />
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <!-- ==================================================================== -->


  <t:titlepage
    t:element="section"
    t:wrapper="fo:block"
  >
    <t:titlepage-content
      font-family="{$title.fontset}"
      t:side="recto"
    >
      <title margin-left="{$title.margin.left}"/>
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <!-- ==================================================================== -->


  <t:titlepage
    font-family="{$title.fontset}"
    t:element="table.of.contents"
    t:wrapper="fo:block"
  >
    <t:titlepage-content t:side="recto">
      <title
        param:key="'TableofContents'"
        t:force="1"
        t:named-template="gentext"
      />
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <t:titlepage
    font-family="{$title.fontset}"
    t:element="list.of.tables"
    t:wrapper="fo:block"
  >
    <t:titlepage-content t:side="recto">
      <title
        param:key="'ListofTables'"
        t:force="1"
        t:named-template="gentext"
      />
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <t:titlepage
    font-family="{$title.fontset}"
    t:element="list.of.figures"
    t:wrapper="fo:block"
  >
    <t:titlepage-content t:side="recto">
      <title
        param:key="'ListofFigures'"
        t:force="1"
        t:named-template="gentext"
      />
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <t:titlepage
    font-family="{$title.fontset}"
    t:element="list.of.examples"
    t:wrapper="fo:block"
  >
    <t:titlepage-content t:side="recto">
      <title
        param:key="'ListofExamples'"
        t:force="1"
        t:named-template="gentext"
      />
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <!-- ==================================================================== -->


</t:templates>
