<t:templates
  xmlns:param="http://nwalsh.com/docbook/xsl/template/1.0/param"
  xmlns:t="http://nwalsh.com/docbook/xsl/template/1.0"
  xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
>


  <!--
  Customized from the original document to rearrange some of the page
  layout and remove the unsupported elements.
  -->


  <t:titlepage
    class="book_titlepage"
    t:element="book"
    t:wrapper="div"
  >
    <t:titlepage-content t:side="recto">
      <title/>
      <abstract/>
      <productname/>
      <copyright/>
      <legalnotice/>
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <t:titlepage
    class="chapter_titlepage"
    t:element="chapter"
    t:wrapper="div"
  >
    <t:titlepage-content t:side="recto">
      <title/>
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <t:titlepage
    class="appendix_titlepage"
    t:element="appendix"
    t:wrapper="div"
  >
    <t:titlepage-content t:side="recto">
      <title/>
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


  <t:titlepage
    class="section_titlepage"
    t:element="section"
    t:wrapper="div"
  >
    <t:titlepage-content t:side="recto">
      <title/>
    </t:titlepage-content>


    <t:titlepage-content t:side="verso">
    </t:titlepage-content>


    <t:titlepage-separator>
    </t:titlepage-separator>


    <t:titlepage-before t:side="recto">
    </t:titlepage-before>


    <t:titlepage-before t:side="verso">
    </t:titlepage-before>
  </t:titlepage>


</t:templates>
