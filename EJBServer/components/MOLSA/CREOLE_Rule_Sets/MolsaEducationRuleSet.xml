<RuleSet name="MolsaEducationRuleSet">
    <Class extends="DynamicEvidence" extendsRuleSet="DynamicEvidenceRuleSet" name="MolsaEducation">
        <Attribute name="startDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="endDate">
            <Annotations/>
            <type>
                <javaclass name="curam.util.type.Date"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="studentStatus">
            <Annotations/>
            <type>
                <codetableentry table="MOLSAEducation"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="educationLevel">
            <Annotations/>
            <type>
                <codetableentry table="MOLSAEducationLevel"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="participant">
            <Annotations/>
            <type>
                <javaclass name="java.lang.Number"/>
            </type>
            <derivation>
                <specified/>
            </derivation>
        </Attribute>
        <Attribute name="related_participant">
            <Annotations/>
            <type>
                <ruleclass name="CaseParticipantRole" ruleset="CaseEntitiesRuleSet"/>
            </type>
            <derivation>
                <reference attribute="caseParticipantRoleRecord">
                    <create ruleclass="CaseParticipantRoleCalculator"
                        ruleset="DynamicEvidenceRuleSet">
                        <reference attribute="participant"/>
                    </create>
                </reference>
            </derivation>
        </Attribute>
    </Class>
</RuleSet>
