<codetables package="curam.codetable">
  <!--Enables UoCPM to specify a Service Offering as "Restrictive" or otherwise. -->
  <codetable
    java_identifier="SPECIALCONDITIONTYPE"
    name="SpecialConditionType"
  >
    <displaynames>
      <locale language="en">Special Condition Type</locale>
    </displaynames>
    <code
      default="false"
      java_identifier="RESTRICTIVE"
      status="ENABLED"
      value="SCT3001"
    >
      <locale
        language="en"
        sort_order="0"
      >
        <description>Restrictive</description>
        <annotation/>
      </locale>
    </code>
  </codetable>
</codetables>
